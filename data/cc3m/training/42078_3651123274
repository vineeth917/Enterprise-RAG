	<script>
		jQuery(document).ready(function($) {
			var ajaxurl = "https://pestleandmortar.us/wp-admin/admin-ajax.php";
			$( document.body).on('click', '.pandm-add-to-cart-button', function(e) {
				e.preventDefault();
				var $this = $(this);
				if( $this.is(':disabled') ) {
					return;
				}
				var id = $(this).data("product-id");
				var data = {
					action     : 'pandm_add_to_cart',
					product_id : id
				};
				$.post(ajaxurl, data, function(response) {
					if( response.success ) {
						$this.text("added to cart");
						$( document.body ).trigger( 'wc_fragment_refresh' );
						//$('.gift-with-coupon').slideUp();
					}
				}, 'json');
			})
			$( document.body).on('click', '.checkout-add-to-cart-button', function(e) {
				e.preventDefault();
				var $this = $(this);
				if( $this.is(':disabled') ) {
					return;
				}
				var id = $(this).data("product-id");
				var data = {
					action     : 'pandm_add_to_cart',
					product_id : id
				};
				$.post(ajaxurl, data, function(response) {
					if( response.success ) {
						$this.text("added to cart");
						$('#gift_with_coupon').addClass("disabled");
						$( document.body ).trigger( 'wc_fragment_refresh' );
						$( document.body ).trigger( 'update_checkout' );
						$( document.body ).trigger( 'added_to_cart' );
						$( document.body ).trigger( 'updated_cart_totals' );
					}
				}, 'json');
			})
		});
	</script>
