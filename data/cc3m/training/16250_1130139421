<script>
function setVideoTitle(ret) {
	var video = $(".videoSide").width();	
	var videoIcon = $(".fa-play-circle").width();
	var title = $(".videoTitle");
	var product = video - (videoIcon + 40);

	title.css({"width": product + "px" });

if (ret != null)
{
var videos = $(".videoSide");
for (var i = videos .length -1; i >= 0; i--)
{
if (videos.length > i)
videos[i].href = "/" + ret[i].Link;
}
}
}
