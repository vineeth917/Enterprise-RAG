<script type="text/javascript">
$(document).one("widgetsloaded", function() {
	setVideoTitle();
	videoControl();
	setTextLinkItem();

	$(window).resize(function() {
		setVideoTitle();
		setTextLinkItem();
	});

	loadAltLogo();

});

function loadAltLogo() {
	var altLogo = "<img class='altLogo' src='https://d7fboktxwazqa.cloudfront.net/MTS_Beta_GoldCountry/0/0/0/0/0/37/37_original'>";
	$(altLogo).prependTo(".logoCtr");
}

function setVideoTitle() {
	var video = $(".videoMain, .videoThird, .videoSide");	
	video.each(function() {
		var videoIcon = $(this).find(".fa-play-circle").width();
		var videoTitleCtr = $(this).find(".videoTitleCtr").width();
		var titleWidth = videoTitleCtr - (videoIcon + 20);

		$(this).find(".videoTitle").outerWidth(titleWidth);
	});	
}

function videoControl() {
	var video = $("video");
	var vt = $(".videoMain, .videoThird");
	vt.on("click", function() {
		var source = $(this).find(".hiddenSrc").html();
		video.find("source").attr("src", source);
		$("video")[0].load();
		video.show();
	});
}

function setCatColor() {
	var category = $(".nineGridThird .category, .gcmHeader");
	category.each(function() {
		var text = $(this).html();
		if(text == "News") {
			$(this).css({"background": "#97361c" });
		} else if (text == "Sports") {
			$(this).css({"background": "#1c973c" });			
		} else if (text == "Opinion") {
			$(this).css({"background": "#3c1c97" });			
		} else if (text == "Lifestyle") {
			$(this).css({"background": "#1c4797" });			
		} else if (text == "Local News") {
			$(this).css({"background": "#971c1c" });		
		} else if (text == "Obituaries") {
			$(this).css({"background": "#B47B4A" });			
		} else if (text == "Classifieds") {
			$(this).css({"background": "#4AB45E" });	
		} else {
			$(this).removeAttr("style");			
		}
	});
}

function setTextLinkItem() {
	var tli = $(".textLinkItem").width();
	var tliWidth = 126;
	var category = $(".textLinkItem .halfGridLink .category");
	var title = $(".textLinkItem .halfGridLink .title");
	var width = tli - tliWidth;

	category.width(width);
	title.width(width);
}

function storyCallback() {
	setTextLinkItem();
	setCatColor();
}
</script>




