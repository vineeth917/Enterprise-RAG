	var urlPath = window.location.pathname.toLowerCase().split('/').filter(function (ret) { return ret.length > 0 ? ret : ""; });

	var site = "";
	var section = "";
	var subscriber = _MTS_userId > 0 ? "yes" : "no";

	if (urlPath.length >= 1)
	{
		if (urlPath[0] === "news-category")
		{
			section = urlPath[1];
		}
		else if (urlPath[0] === "portal.aspx")
		{
			section = "homepage";
		}
		else if (urlPath[0] === "live-content")
		{
			switch (urlPath[1])
			{
				case "auburn-journal":
				case "lincoln-news-messenger":
				case "the-folsom-telegraph":
				case "the-press-tribune":
				case "placer-herald":
				case "the-loomis-news": 
					site = urlPath[1].replace(/-/g, ' ');
					break;
				default:
					break;
			}
		}
	}
	else
	{
		section = "homepage";
	}

	googletag.pubads().setTargeting("site", site);
	googletag.pubads().setTargeting("section", section);
	googletag.pubads().setTargeting("subscriber", subscriber);

    googletag.pubads().enableSingleRequest();
    googletag.pubads().collapseEmptyDivs();
    googletag.enableServices();
  });
</script>

