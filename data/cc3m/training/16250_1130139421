<script type="text/javascript">// <![CDATA[
$(document).one("widgetsloaded", function() {
  var tab = $(".tabLink");
  var con = $(".tabContent");

  if(window.location.href.indexOf("auburn-journal") > 0) {
    $(".twitterHeader").attr("href", "https://twitter.com/auburnjournal");
    $(".tabContent").hide();
    $("#tabLink_0_Content").show();
  } else if (window.location.href.indexOf("the-press-tribune") > 0) {
    $(".twitterHeader").attr("href", "https://twitter.com/RosevillePT");
    $(".tabContent").hide();
    $("#tabLink_1_Content").show();
  } else if (window.location.href.indexOf("lincoln-news-messenger") > 0) {
    $(".twitterHeader").attr("href", "https://twitter.com/lincolnnm");
    $(".tabContent").hide();
    $("#tabLink_2_Content").show();
  } else if (window.location.href.indexOf("the-loomis-news") > 0) {
    $(".twitterHeader").attr("href", "https://twitter.com/TheLoomisNews");
    $(".tabContent").hide();
    $("#tabLink_4_Content").show();
  } else if (window.location.href.indexOf("placer-herald") > 0) {
    $(".twitterHeader").attr("href", "https://twitter.com/theplacerherald");
    $(".tabContent").hide();
    $("#tabLink_5_Content").show();
  } else if (window.location.href.indexOf("the-folsom-telegraph") > 0) {
    $(".twitterHeader").attr("href", "https://twitter.com/FolsomTelegraph");
    $(".tabContent").hide();
    $("#tabLink_6_Content").show();
  } else {
    $(".twitterHeader").attr("href", "https://twitter.com/auburnjournal");
    $(".tabContent").hide();
    $("#tabLink_0_Content").show();  
  }

  var maxHeight = -1;

  tab.each(function() {
    maxHeight = maxHeight > $(this).outerHeight() ? maxHeight : $(this).outerHeight();
  });

  var dropHeight = maxHeight * tab.length;

  $(".dropTab").on("click", function() {
    var tabs = $(".tabs");
    tabs.toggleClass("showTabs");
    tabs.hasClass("showTabs") ? tabs.height(dropHeight) : tabs.removeAttr("style");
  });

  tab.on("click", function() {
    var id = $(this).attr("id");
    con.hide();
    $("#" + id + "_Content").show();
    var anchor = $(this).html().split("@")[1];
    $(".twitterHeader").attr("href", "https://twitter.com/" + anchor);
  });

  var color = $(".mts_cc_button").css("color");
  var background = $(".mts_cc_button").css("background-color");

  tab.hover(function() {
     $(this).css({"background": background });
     $(this).css({"color": color });
  },
  function() {
     $(this).removeAttr("style");
  });
});
// ]]></script>







