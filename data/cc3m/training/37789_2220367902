/* FSRI List Grid Fix */
ul.fsri-list {
  /* Adjust columns as you wish: 
     e.g., repeat(3, 1fr) for exactly 3 columns, 
     or repeat(auto-fill, minmax(250px, 1fr)) for a responsive grid. */
  display: grid;
  grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));
  gap: 10px;             /* Space between items horizontally and vertically */
  align-items: stretch;  /* Ensures each item in a row matches the tallest item */
}

ul.fsri-list > li {
  /* Let each grid cell expand to match the row height */
  display: flex;
  flex-direction: column;
  overflow: visible !important;
}

ul.fsri-list > li a {
  display: flex;
  flex-direction: column;
  height: 100%;          /* Make the link fill the entire grid cell */
  box-shadow: 0px 0px 10px lightgray;
  border-radius: 5px;
  padding: 5px;
  transition: box-shadow 0.3s ease;
}

ul.fsri-list > li a:hover {
  box-shadow: 0px 0px 15px gray;
}

ul.fsri-list > li a img {
  position: relative;
  transition: transform 0.3s ease;
  max-width: 100%;
  height: auto;          /* Maintain the image’s aspect ratio */
}

ul.fsri-list > li a:hover img {
  transform: scale(1.05);
}

ul.fsri-list > li a img::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 3px;
  background-color: #f44336;
}

