function observeSection(){const targetNode=document.querySelector(".main");const config={childList:true};const callback=(mutationList,observer)=>{for(const mutation of mutationList){if(mutation.type==="childList"){let section=document.querySelectorAll(".main section.snap-always");let pageCount=section.length;if(mutation.nextSibling&&["feed-paging","mvk-feed-early-ajax"].indexOf(mutation.nextSibling.id)>-1){mavDefineFeedAdsHatches();}}}
