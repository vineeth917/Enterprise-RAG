Array.prototype.forEach.call(container,(v,k)=>{v.classList.add("ins");if(!((k+1)%multiplied)){let injectedElement=v.nextElementSibling;let i=0;for(const prop in config){if(config.hasOwnProperty(prop)){if(!config[prop]['slotCreated']){MavCreateContainerAds(prop,injectedElement);definedFeedSlot(prop,(screen===1));config[prop]['slotCreated']=true;i++;}
