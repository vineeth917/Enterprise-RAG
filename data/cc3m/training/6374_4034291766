<script language="javascript">
jQuery(function($) {
  $(document).ready(function() {
	initNewsSlider();
  });
  
  $(window).on('resize', function() {	
//	initNewsSlider();
  }).trigger('resize');
  
  function initNewsSlider() {
	$('#news-slider').each(function() {
		var $this = $(this);
		if ($this.children().length > 1) {
			
			$this.slick({
				dots: true,
				arrows: false,
				infinite: false,
				speed: 500,
				fade: false,
				cssEase: 'linear',
				slidesToShow: 4,
				slidesToScroll: 1,
				responsive: [
					{
						breakpoint: 992,
						settings: {
						  slidesToShow: 2
						}
					}
				]
			});
		
		
			const newsTab = document.querySelector('#news-tab')
			newsTab.addEventListener('shown.bs.tab', event => {
			  $('#news-slider').slick('setPosition');
			});
			
			const newsCollapsible = document.getElementById('news-collapse')
			newsCollapsible.addEventListener('shown.bs.collapse', event => {
			  $('#news-slider').slick('setPosition');
			});
			
			if ($(window).width() < 767) {
				$('#news-slider').slick('unslick');
			}
				
		}
		
	});
	
	}
});

</script>

