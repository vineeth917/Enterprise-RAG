<script type="text/javascript">
 
	jQuery(function($) {
		$(document).ready(function() {
			//set tab heights consistently across tabs based on tallest one
			var tc = $('.tab-content[data-val="xStHIm9cPJ"');
			var i;
			var maxHeight = 150;
			for (i = 0; i < tc.length; i++) {
				var origBlock=false;
				if (jQuery(tc[i]).css('display') == 'block') {
					jQuery(tc[i]).hide();
					origBlock = true;
				}
				
				var height = jQuery(tc[i]).height();
				if (height>1000) {
					var origDisplay = jQuery(tc[i]).css('display');
					jQuery(tc[i]).css('display','block');
					height = jQuery(tc[i]).height();
					jQuery(tc[i]).css('display',"");
				}
				if (height>maxHeight) {
					maxHeight = height;
				}
				if (origBlock) {
					jQuery(tc[i]).show();
				}
			}
			for (i = 0; i < tc.length; i++) {
				//neeed to fix to keep tabs at same height
				jQuery(tc[i]).find(".bigscreen").height(maxHeight + 'px');
			}
			
			//var acc = document.getElementsByClassName("accordion");
			var acc=$('.accordion[data-val="xStHIm9cPJ"');
			var i;
			for (i = 0; i < acc.length; i++) {
			  acc[i].onclick = function() {
				accordion_togglex($(this));
			  }
			  acc[i].onkeypress = function(event) {
				if (event.which == 13 || event.keyCode == 13) {
					accordion_togglex($(this));
				}
			  }
			}
		});
	});
</script> 
