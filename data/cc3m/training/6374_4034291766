	<script type="text/javascript">
	jQuery(function($) {
		$(document).ready(function() {
			var arrTabbedImages = ["/sites/default/files/assets/spring-flowers-GC-homepage-hero.png","/sites/default/files/assets/Spring2025-greatfalls-home.jpg","/sites/default/files/assets/hero-trailriding-4000.jpg","/sites/default/files/assets/Spring2025-greenspringgarden_0.png"];
			var arrTabbedPosition = ["center","center","center","center"];  
			var arrTabbedHasVideo = [];
			var arrTabbedVideos = [];
			
							var arrThisVideos = [];
									arrTabbedHasVideo.push(false);
								arrTabbedVideos.push(arrThisVideos);
							var arrThisVideos = [];
									arrTabbedHasVideo.push(false);
								arrTabbedVideos.push(arrThisVideos);
							var arrThisVideos = [];
									arrTabbedHasVideo.push(false);
								arrTabbedVideos.push(arrThisVideos);
							var arrThisVideos = [];
									arrTabbedHasVideo.push(false);
								arrTabbedVideos.push(arrThisVideos);
						
			//pick random
			var size = arrTabbedImages.length;
			var x = Math.floor(size*Math.random());
			
			//add videos
			if (arrTabbedHasVideo[x]) {
				//$(".video_background").show(); 
				$(".focalpoint-overlay").hide();
				$(".image_background").hide();
				var vidSrcs = "";
				for (var vids in arrTabbedVideos[x]) {
					for (var vid in vids) {
						var src = arrTabbedVideos[x][vids[vid]];
						var srcType = src.split('.').pop();
						vidSrcs += "<source src=\"" + src + "\" type=\"video/" + srcType + "\"> \r\n";
					}
				}
				$(".video_background").attr('poster', arrTabbedImages[x]);
				$(".video_background").html(vidSrcs);
				$(".video_background").fadeIn("slow", function() {
					
				});
				
				$(".video_background").css('margin-top',''); 
			} else {
				//or add image
				$(".image_background").hide();
				$(".video_background").hide(); 
				$('.image_background').css("background-image", "url('" + arrTabbedImages[x] + "')");
				$('.image_background').css("background-position", arrTabbedPosition[x] );
				$(".image_background").fadeIn( "slow", function() {
					
				});
				
			}
			
		});
	});
</script>
	
