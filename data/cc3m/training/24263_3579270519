// Redirect gallery categories
jQuery(document).ready(function($) {
  $('a').each(function() {
    var href = $(this).attr('href');
    if (typeof href !== 'undefined' && href.includes("gallery-category")) {
      href = href.replace(/gallery-category\/(.+)\//, "galleries/?categ=$1");
      //console.log(href);
      $(this).attr('href', href);
    }
  });
});
    
