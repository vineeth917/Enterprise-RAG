<script>
  document.addEventListener("DOMContentLoaded", function (){
    const mobileLeaguesContainer = document.querySelector(".mobile-leagues-container");
    const mobileNavigationMenu = document.querySelector(".mobile-navigation-menu ul.list-items li.level-1 span");

    const mobileMenuContainer = document.querySelector("#mobile-menu-container");
    const mobileMenuTrigger = document.querySelector("#main-menu-trigger");
    const itemWithChildren = [...document.querySelectorAll("li.has-children span.with-child")];
    const itemWithGranhildren = [...document.querySelectorAll("li.has-grandchildren > a.child-link")];
    const wrapper = document.querySelector('#mobile-menu-container .mobile-menu-wrapper');
    const backButtonsLevel2 = [...document.querySelectorAll('#mobile-menu-container .children-container > .back-button-container .back-button')];
    const backButtonsLevel3 = [...document.querySelectorAll('#mobile-menu-container .grandchildren-container > .back-button-container .back-button')];
    const searchBox = document.querySelector('#primary-searchbox');
    const header = document.querySelector('header')
    const menuIcon = document.getElementById('menuIcon');
    const closeIcon = document.getElementById('closeIcon');

    let itemWithChildrenActive,
        itemWithGrandChildrenActive;

    mobileMenuTrigger.addEventListener("click", function(){


      mobileMenuContainer.classList.toggle("open");
      document.querySelector('body').classList.toggle('block-scroll');
      const headerColorScheme = 'white'
      if(headerColorScheme == 'black' || headerColorScheme == 'transparent'){
        if(mobileMenuContainer.classList.contains("open")){
          header.classList.remove(`color-scheme-${headerColorScheme}`)
          header.classList.add('color-scheme-white')
          document.querySelector('#logo_black').style.display = 'block'
          document.querySelector('#logo_white').style.display = 'none'
        } else{
          header.classList.remove('color-scheme-white')
          header.classList.add(`color-scheme-${headerColorScheme}`)
          document.querySelector('#logo_black').style.display = 'none'
          document.querySelector('#logo_white').style.display = 'block'
        }
      }
      searchBox.classList.remove('visible');
      // Replace the menu icon.
      menuIcon.classList.toggle('hidden-nav-item');
      closeIcon.classList.toggle('hidden-nav-item');
    });

    itemWithChildren.map(element => {
      element.addEventListener("click", function(e){
        mobileLeaguesContainer.classList.add("v-hidden");
        mobileNavigationMenu.classList.add("v-hidden");
        if (mobileLeaguesContainer.classList.contains("v-hidden")) {
            setTimeout(() => {
                e.preventDefault();
                console.log('itemWithChildren click ---');
                this.parentElement.classList.add('open');
                wrapper.classList.add('open');
                itemWithChildrenActive = this;
            }, 10);
        }
      });
    });


    itemWithGranhildren.map(element => {
      element.addEventListener("click", function(e){
        e.preventDefault();
        const dataId = element.getAttribute("href").split('/')[2].trim();
        const isTeamMenu = element.classList.contains('type-teams');
        this.parentElement.classList.add('open');
        itemWithGrandChildrenActive = this;

        // Update the promo images
        if (isTeamMenu) {
          const promoItems = document.querySelectorAll(`.promo-item.type-teams`);
          const promoItem = document.querySelector(`.${dataId}-mobile-promo`);

          if (promoItem) {
            promoItems.forEach(promo => {
              if (promo.classList.contains(`${dataId.toLowerCase()}-promo`)) {
                promo.classList.remove('hidden')
              } else {
                promo.classList.add('hidden')
              }
            });
          }
        }

      });
    });

    backButtonsLevel2.map(element => {
      element.addEventListener("click", function(e){

        if (mobileLeaguesContainer.classList.contains("v-hidden")) {
            mobileLeaguesContainer.classList.remove("v-hidden");
            mobileNavigationMenu.classList.remove("v-hidden");
            setTimeout(() => {
                e.preventDefault();
                itemWithChildrenActive.parentElement.classList.remove('open');
                wrapper.classList.remove('open');
            }, 100);
        }
      });
    });

    backButtonsLevel3.map(element => {
      element.addEventListener("click", function(e){
        e.preventDefault();
        console.log('------ grand click back ---');
        itemWithGrandChildrenActive.parentElement.classList.remove('open');
      });
    });

  });
</script>
<script>
  document.addEventListener("DOMContentLoaded", function() {
    const listItems = document.querySelectorAll(".team-item-navigation .nav-container .list-item");
    const teamContainers = document.querySelectorAll(".teams-list-container .team-list");
    const collectionsTeamsContainer = document.querySelectorAll(".collection-team-list-container")

    listItems.forEach(function(item) {
      item.addEventListener("mouseover", function() {
        let dataId = item.getAttribute("data-league-name");

        teamContainers.forEach(function(container) {
          if (container.getAttribute("data-league-name") === dataId) {
            container.classList.remove("hidden");
          } else {
            container.classList.add("hidden");
          }
        });

        collectionsTeamsContainer.forEach(function(container) {
          if (container.getAttribute("data-league-name") === dataId) {
            container.classList.remove("hidden");
          } else {
            container.classList.add("hidden");
          }
        });

        listItems.forEach(function(otherItem) {
          const promoItems = document.querySelectorAll(`.is-team-nav.promo-item`);
          const promoItem = document.querySelector(`.is-team-nav.${dataId.toLowerCase()}-promo`);

          otherItem.classList.toggle('active', otherItem === item);
          if (promoItem) {
            promoItems.forEach(promo => {
              if (promo.classList.contains(`${dataId.toLowerCase()}-promo`)) {
                promo.classList.remove('hidden')
              } else {
                promo.classList.add('hidden')
              }
            });
          }
        });
      });
    });
  });
</script>
<script>
  document.addEventListener("DOMContentLoaded", function(){
    const searchInput = document.querySelector('#primary-searchbox #search-input');
    let previousValue = searchInput.value;

    searchInput.addEventListener("input", function () {
      const currentValue = searchInput.value;
      if (currentValue.length > previousValue.length) {
        
      } else if (currentValue.length < previousValue.length) {

      }
      previousValue = currentValue;
    });

  });

  // Show / Hide Search Bar

  document.addEventListener("DOMContentLoaded", function(){
    const searchIcon = document.querySelector('.top-right-menu .search-icon-item');
    const searchBar = document.querySelector('.top-right-menu .search-bar-item');
    const mobileSearchBar = document.querySelector('#mobile-search');
    const searchBox = document.querySelector('#primary-searchbox');
    const mainContainer = document.querySelector('#MainContent');
    const mobileMenuContainer = document.querySelector("#mobile-menu-container");
    const searchInpunt = document.querySelector("#search-input");

    const showSearch = () => {
      mainContainer.classList.toggle('showing-search');
      searchBox.classList.toggle('visible');
      searchInpunt.focus();
      mobileMenuContainer.classList.remove('open');
    }

    searchIcon.addEventListener('click', function(e){
      e.preventDefault();
      showSearch()
    });
    searchBar.addEventListener('click', function(e){
      e.preventDefault();
      showSearch()
    });
    mobileSearchBar.addEventListener('click', function(e){
      e.preventDefault();
      showSearch()
    });


    document.addEventListener('click', function(event) {
      const container = document.querySelector('.header-search__container');
      const parent = event.target.closest('.header-search__container');

      if (container.contains(event.target) || parent?.contains(event.target)) {
        // Clicked in box
      } else {
        const searchIcon = event.target.closest('.search-icon-item');
        const searchBar = event.target.closest('.search-bar-item');
        const mobileSearchBar = event.target.closest('#mobile-search');
        if (!searchIcon && !searchBar && !mobileSearchBar) {
          searchBox.classList.remove('visible');
          mainContainer.classList.remove('showing-search');
        }
      }
    });

  });
</script>

<script>
  // Changing the top of header depeding if is visible the announcement bar
  document.addEventListener("DOMContentLoaded", function(){
    headerv2();
    window.addEventListener("resize", headerv2, false);
  });
  function headerv2(event) {
    const announcementBar = document.querySelector("#shopify-section-announcement-bar");
    const header = document.querySelector("#shopify-section-header-v2");

    if(announcementBar) {
      header.style.top = `${announcementBar.getBoundingClientRect().height}px`
    } else {
      header.style.top = `0`
    }
  }
</script>
<script>
  //Change color to white on scroll if color-scheme = black or transparent
  const headerColorScheme = 'white'
  const header = document.querySelector('header')

  const showWhiteHeader = () => {
    if(header.classList.contains(`color-scheme-transparent`)){
      header.style.marginBottom = '-72px'
      header.classList.add('color-scheme-white')
      header.classList.remove(`color-scheme-transparent`)
      document.querySelector('#logo_black').style.display = 'block'
      document.querySelector('#logo_white').style.display = 'none'
    }
  }

  const showTransparentHeader = () => {
    if(!header.classList.contains(`color-scheme-transparent`)){
        header.style.marginBottom = '-72px'
        header.classList.add(`color-scheme-${headerColorScheme}`)
        header.classList.remove('color-scheme-white')
        document.querySelector('#logo_black').style.display = 'none'
        document.querySelector('#logo_white').style.display = 'block'
    }
  }

  if(headerColorScheme == 'transparent'){
    document.addEventListener('scroll', () => {
      if(window.scrollY > 250){
        showWhiteHeader()
      } else{
        showTransparentHeader()
      }
    })

    header.addEventListener('mouseover', () => {
      if(window.scrollY <= 250){
        showWhiteHeader()
      }
    })

    header.addEventListener('mouseout', ()=>{
      if(window.scrollY <= 250){
        showTransparentHeader()
      }
    })
  }
</script>
