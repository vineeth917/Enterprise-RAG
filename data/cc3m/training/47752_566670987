<script>

  const addKeyframe_template__18826542579939__15fea79d_2768_48c7_8559_46cacc1ef892 = () => {
    const promosContainer = document.querySelector('#shopify-section-template--18826542579939__15fea79d-2768-48c7-8559-46cacc1ef892 .promo_animated__container');

    const promosContent = promosContainer.innerHTML;
    promosContainer.innerHTML += promosContent;

    const keyframes = `\
        @keyframes scroll-mobile {\
          0% {\ transform: translateX(0);\ }\
          100% {\ transform: translateX(-${promosContainer.scrollWidth / 2}px);\}\
        }\
    `;

    const promoSection = document.querySelector('#shopify-section-template--18826542579939__15fea79d-2768-48c7-8559-46cacc1ef892 .promo_animated__promo_section');

    const keyframesStylesCointainer = document.createElement('style');

    keyframesStylesCointainer.innerHTML = keyframes;

    promoSection.appendChild(keyframesStylesCointainer);

  }

  addKeyframe_template__18826542579939__15fea79d_2768_48c7_8559_46cacc1ef892();
