<script>
  function normalizeTeam(team_tag){
    let team = team_tag;
    const upcaseNames = ['pga', 'liv', 'us', 'ac', 'bc','cf', 'fc', 'gc','sc', 'xi', 'xii', 'xiii', 'byu','la', 'lsu', 'wnba','mlb','nfl','nba','milb', 'nascar','rcr', 'tcu', 'usa','am', 'ucla','usc','twenty', 'thirty', 'forty', 'fifty']
    const replaceNames = [['4aces','4Aces'], ['a-frame','A-Frame'], ['hyflyers', 'HyFlyers'], ['mclaren','McLaren'],['rangegoats', 'RangeGoats'],['d.c.', 'D.C.'],['66ers', '66Ers'],['89ers', '89Ers'],['a&m','A&M']]

    team = team_tag?.toLowerCase().split(' ')?.map(word => {
        if(word === 'none') return ''

        const nameToReplace = replaceNames.find(name => word === name[0])
        const upcaseName = upcaseNames.some(name => name === word)
        if(nameToReplace){
          return word.replace(nameToReplace[0], nameToReplace[1])
        } else if(upcaseName){
          return word.toUpperCase()
        } else{
          return word.charAt(0)?.toUpperCase() + word.slice(1)
        }
    })?.join(' ') || team_tag;
    return team
  }

  function addRecommendationsToMiniCart(recsContainer, recsCardsContainer, url) {
    fetch(url)
      .then((response) => response.json())
      .then((data) => {
        let cardsHTML = '';

        for (const product of data.products) {
          let team_tags = product.tags.filter(function (str) {
            return str.includes('team:');
          });
          const team_tag = team_tags[0]?.toLowerCase().replace('team:', '');
          const team = normalizeTeam(team_tag);

          const regex = /\b(\d+)([a-zA-Z]{5,})\b/gi;
          let displayTitle = team ? product.title.replace(team.trim(), '') : product.title;
          displayTitle = displayTitle.replace(regex, '<b>$1</b>$2');

          const cardHTML = `<div class="slider-card swiper-slide" product_id="${product.id}">
            <a class="product-rec-link" href="${product.url}">
              <img src="${product.featured_image}" alt="${product.title}" width="auto" height="auto"/>
              <div class="slider-card__caption">
                <p class="slider-card__subheading"> ${team} <span class="card-price">${formatToMoneyPrice(product.price)}</span></p>
                <p class="slider-card__heading"> ${displayTitle}</p>
              </div>
            </a>
          </div>`;
          cardsHTML += cardHTML;
        }

        recsCardsContainer.innerHTML = cardsHTML;

        recsContainer.classList.remove('hidden');
      })
      .catch((e) => {
        console.error(e);
        recsContainer.classList.add('hidden');
      });
  }

  const recsContainers = document.querySelectorAll('#cart-product-recommendations');
  if (recsContainers?.length) {
    recsContainers?.forEach((recsContainer) => {
      if (recsContainer?.getAttribute('product-id')?.length > 0) {
        const recsCardsContainer = recsContainer?.querySelector('#slider--cart');
        let url = recsContainer.dataset.url + recsContainer.getAttribute('product-id');
        addRecommendationsToMiniCart(recsContainer, recsCardsContainer, url);

        const swiper = new Swiper(`#cart-product-recommendations[data-section-id=""] .swiper`, {
          centerInsufficientSlides: true,
          watchOverflow: true,
          spaceBetween: 6,
          slidesPerView: 'auto',
          breakpoints: {
           0:{
              slidesPerGroup: 1
            },
            640: {
              slidesPerGroup: 3
            },
          },
          navigation: {
            prevEl: `#cart-product-recommendations[data-section-id=""] .slider--cart__buttons__actions #button--prev`,
            nextEl: `#cart-product-recommendations[data-section-id=""] .slider--cart__buttons__actions #button--next`,
          },
        });
      }
    });
  }

</script></div>



