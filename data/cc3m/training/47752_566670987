<script>
  class CartNote extends HTMLElement {
    constructor() {
      super();

      this.addEventListener('change', debounce((event) => {
        const body = JSON.stringify({ note: event.target.value });
        fetch(`${routes.cart_update_url}`, {
          ...fetchConfig(),
          ...{ body }
        });
      }, 300))
    }
  }

  customElements.define('cart-note', CartNote);

  // Custom minicart

  const bubble = document.getElementById('cart-icon-bubble');
  const closeButtonMini = document.querySelector('.js-mini-cart__close');
  const miniOverlay = document.querySelector('.mini-cart-overlay');

  // bubble.addEventListener('click', openMiniCart);
  closeButtonMini.addEventListener('click', closeMiniCart);
  miniOverlay.addEventListener('click', closeMiniCart);

  function openMiniCart(evt) {
    evt.preventDefault();

    if (document.querySelector('body') != null) {
      document.querySelector('body').classList.add('overflow-hidden');
    }

    if (document.querySelector('.mini-cart-overlay') != null) {
      document.querySelector('.mini-cart-overlay').classList.add('mini-cart-overlay--open');
    }

    if (document.querySelector('#mini-cart') != null) {
      document.querySelector('#mini-cart').classList.add('mini-cart--open');
    }
  }

  function closeMiniCart(evt) {
    evt.preventDefault();

    if (document.querySelector('body') != null) {
      document.querySelector('body').classList.remove('overflow-hidden');
    }

    if (document.querySelector('.mini-cart-overlay') != null) {
      document.querySelector('.mini-cart-overlay').classList.remove('mini-cart-overlay--open');
    }

    const miniCart = document.querySelector('#mini-cart');

    if (miniCart != null) {
      miniCart.classList.remove('mini-cart--open');
      removeTrapFocus(miniCart);
    }
  }

  // END custom minicart
  document.addEventListener('DOMContentLoaded', function() {
    function isIE() {
      const ua = window.navigator.userAgent;
      const msie = ua.indexOf('MSIE ');
      const trident = ua.indexOf('Trident/');

      return (msie > 0 || trident > 0);
    }

    if (!isIE()) return;
    const cartSubmitInput = document.createElement('input');
    cartSubmitInput.setAttribute('name', 'checkout');
    cartSubmitInput.setAttribute('type', 'hidden');
    document.querySelector('#cart').appendChild(cartSubmitInput);
    document.querySelector('#checkout').addEventListener('click', function(event) {
      document.querySelector('#cart').submit();
    });
  });

  const cartIcon = document.querySelector('#cart-icon-bubble');

  cartIcon.addEventListener('click', function(e){
    e.preventDefault();
    document.querySelector('body')?.classList.add('overflow-hidden');
    const miniCart = document.querySelector('#mini-cart');
    if (miniCart) {
      miniCart.classList.add('mini-cart--open');
      trapFocus(miniCart, miniCart.querySelector('.mini-cart__close'));
      document.querySelector('.mini-cart-overlay')?.classList.add('mini-cart-overlay--open');
    }
  });
</script>



<script src="//www.neweracap.com/cdn/shop/t/3354/assets/cart-notification.js?v=117226812370751283931745166434" defer="defer"></script>
