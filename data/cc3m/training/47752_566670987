    <script>
      window.addEventListener('load', function() {
        var iframe = document.getElementById('preview-bar-iframe');
        var nextImages = [];
        if (iframe) {
            var nextSibling = iframe.nextElementSibling;
            while (nextSibling && nextImages.length < 2) {
                if (nextSibling.tagName.toLowerCase() === 'img') {
                    nextImages.push(nextSibling);
                }
                nextSibling = nextSibling.nextElementSibling;
            }
            for (var i = 0; i < nextImages.length; i++) {
              if (!nextImages[i].getAttribute("alt")) {
                nextImages[i].setAttribute("alt", "");
              }
            }

            const iframeContent = iframe.contentDocument || iframe.contentWindow.document;
            var uiStackItem = iframeContent.querySelector('.admin-bar .ui-stack-item');
            uiStackItem.setAttribute('role', 'alert');
        }
      });
    </script>
    <!--
      END
      ACCESSIBILITY - ADA Fixes
    -->
    <script src="//www.neweracap.com/cdn/shop/t/3354/assets/react-apps.js?v=45392028338237631431745166491" defer="defer"></script>
    <script>
    document.addEventListener('DOMContentLoaded', () => {
      boldSilhouettes()
    })
    </script>
