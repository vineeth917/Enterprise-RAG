<script>
  window.addEventListener('load', function() {
        function checkForDynamicElement() {
          const inputElement1 = document.querySelector('.klaviyo-form-RRvFMh.form-version-cid-1 input.needsclick');
          const labelElement1 = document.querySelector('.klaviyo-form-RRvFMh.form-version-cid-1 label.needsclick');
          if (inputElement1 && labelElement1) {
            const inputId1 = inputElement1.getAttribute('id');
            inputElement1.setAttribute("id", inputId1 + "_fv-cid-1");
            labelElement1.setAttribute("for", inputId1 + "_fv-cid-1");
          } else {
            setTimeout(checkForDynamicElement, 100);
          }
        }
        checkForDynamicElement();
  });
</script>
