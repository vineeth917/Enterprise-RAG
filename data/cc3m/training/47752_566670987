<script>
  window.addEventListener('load', function() {
        function checkForDynamicElement() {
          const inputElement2 = document.querySelector('.klaviyo-form-RRvFMh.form-version-cid-2 input.needsclick');
          const labelElement2 = document.querySelector('.klaviyo-form-RRvFMh.form-version-cid-2 label.needsclick');
          if (inputElement2 && labelElement2) {
            const inputId2 = inputElement2.getAttribute('id');
            inputElement2.setAttribute("id", inputId2 + "_fv-cid-2");
            labelElement2.setAttribute("for", inputId2 + "_fv-cid-2");
          } else {
            setTimeout(checkForDynamicElement, 100);
          }
        }
        checkForDynamicElement();
  });
</script>
