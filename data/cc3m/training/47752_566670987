      //Remove Osano's styles from the Preview Bar
      window.addEventListener('DOMContentLoaded', () => {
        const previewBar = document.querySelector('#preview-bar-iframe');
        if (previewBar && previewBar.style?.display === 'none') {
          previewBar.style = 'position: fixed; bottom: 0px; left: 0px; z-index: 2147483647; width: 100%; border: none; box-shadow: rgba(0, 0, 0, 0.2) 0px -1px 3px; overflow: hidden; transform: translateY(0px); transition: transform 300ms ease 0s; height: 92px;';

          // To fix the ADA issue related to the preview bar images.
          const previewBarImages = previewBar.querySelectorAll('img');
          if (previewBarImages && previewBarImages.length > 0) {
            previewBarImages.forEach(function(image) {
              if (image && image.style?.display === 'none') {
                image.setAttribute('aria-hidden', true);
              }
            });
          }
        }
      });
