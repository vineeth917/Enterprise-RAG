<script>
  (function() {
  function loadSilhouettesTypeSlider() {
    const carouselSection = document.querySelector(`[data-silhouette-slider-section-id="template--18826542579939__silhouettes_type_slider_ErkrUx"] .swiper`);
    const swiper = new Swiper(`[data-silhouette-slider-section-id="template--18826542579939__silhouettes_type_slider_ErkrUx"] .swiper`, {
      watchOverflow: true,
      slidesPerView: 'auto',
      breakpoints: {
        0: {
          pagination:{
            enabled: true
          }
        },
        768:{
          pagination: {
            enabled: false
          }
        }
      },
      pagination: {
        el: `[data-silhouette-slider-section-id="template--18826542579939__silhouettes_type_slider_ErkrUx"] .m-silhouette-type-slider__dots .swiper-pagination`,
        clickable: true,
        renderBullet: function (index, className) {
          return `<span class="${className}"></span>`;
        },
      },
      navigation: {
        enabled: true,
          prevEl: `[data-silhouette-slider-section-id="template--18826542579939__silhouettes_type_slider_ErkrUx"] .m-silhouette-type-slider__buttons__actions .prevArrow`,
          nextEl: `[data-silhouette-slider-section-id="template--18826542579939__silhouettes_type_slider_ErkrUx"] .m-silhouette-type-slider__buttons__actions .nextArrow`,
      },
      grabCursor: true,
      keyboard: true,
      mousewheel: {
        enabled: true,
        forceToAxis: true,
      },
    });
  }

  window.addEventListener('DOMContentLoaded', function() {
    loadSilhouettesTypeSlider()
  })})();
</script>


</section>

      

