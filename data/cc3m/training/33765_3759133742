    <script defer="true" src="https://static1.squarespace.com/static/vta/5c5a519771c10ba3470d8101/scripts/site-bundle.659eafb12594f8f1df099d7a645b3210.js" type="text/javascript"></script>
    <script>
window.addEventListener("load", function()
{
  var images = document.querySelectorAll("img"); // Select all <img> elements
  images.forEach(function(image)
  {
    image.classList.add("no-select");
    image.addEventListener("dragstart", protectImage);
    image.addEventListener("contextmenu", protectImage);

    var filename = image.src.split('/').pop();
    if (filename.endsWith("-c.jpg"))
    {
      var container = document.createElement("div");
      container.classList.add("clipped-image");
      var imageClone = image.cloneNode(true);
      container.appendChild(imageClone);
      container.style.height = (image.height - 20) + "px";
      image.parentNode.replaceChild(container, image);
    }
  
  });
});

function protectImage(e) {
  alert("This image is copyright of Steve Bloom. Please get in touch if you would like to arrange a licence to use it.");
  e.preventDefault();
}
/*
document.addEventListener("contextmenu", (e) => {
  if (e.target.tagName === 'IMG') {
    protectImage(e);
    return false;
  }
