<!-- stop more than one day for bookings for paddling pool -->
<script>
document.addEventListener("DOMContentLoaded", function () {
  const targetCategory = "Pool"; 
  const eventCategory = document.querySelector(".event-categories").innerText.trim();

  if (eventCategory === targetCategory) {
    const ticketSelects = document.querySelectorAll(".em-ticket-select");

    function disableOtherTickets(selectedTicket) {
      ticketSelects.forEach((ticketSelect) => {
        if (ticketSelect !== selectedTicket && selectedTicket.value > 0) {
          ticketSelect.disabled = true;
        } else {
          ticketSelect.disabled = false;
        }
      });
    }

    ticketSelects.forEach((ticketSelect) => {
      ticketSelect.addEventListener("change", function () {
        disableOtherTickets(ticketSelect);
      });
    });
  }
});
</script>


