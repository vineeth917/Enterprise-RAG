                                                        target: open_in_new_window ? '_blank' : '',
                                                        tabindex: $parent.isItemOpen() ? 0 : -1
                                                    },
                                                    event: {
                                                        focus: function() { $parents[1].openMenu(); $parent.openMenuItem(); },
                                                        blur:  function() { $parents[1].closeMenu(); $parent.closeMenuItem(); },
