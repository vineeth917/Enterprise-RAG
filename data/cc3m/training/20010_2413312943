                                                    $parent.closeMenu();
                                                    document.querySelector('.c-navigation__toggle--sport').focus();
                                                }
                                            }
                                        },"></a>
                <!-- /ko -->

                <!-- ko if: items && items.length -->
                <div class="c-navigation__level-2"
                    data-bind="css: isItemOpen() ? 'is-open' : '', attr: {'aria-hidden': isItemOpen() ? 'false' : 'true'}">
