  <script>
    jQuery('#product-selector').change(function () {
      const val = jQuery(this).val()
      jQuery('.product-details, .product-map').hide();
      jQuery('.product-details[data-product=' + val + ']').show();
      jQuery('.product-map[data-product=' + val + ']').show();
    });
      jQuery("#search-form").on('submit',function(e){
      e.preventDefault();
      redirect_to_coveo('middle_form');
  });
  jQuery("#search-button").on('click',function(e){
      e.preventDefault();
      redirect_to_coveo('middle_form');
  });
  jQuery("#header-form").on('submit',function(e){
      e.preventDefault();
      redirect_to_coveo('header_form');
  });
  jQuery("#header-search-button").on('click',function(e){
      e.preventDefault();
      redirect_to_coveo('header_form');
  });
  function redirect_to_coveo(form_name){
    var search_term = "";
    if(form_name == 'middle_form'){
      search_term = jQuery("#search-box").val();
    }
    else if(form_name == 'header_form'){
      search_term = jQuery("#header-search-box").val();
    }
    window.location.href = "https://customer.precisely.com/s/global-search/%40uri#q="+search_term;
  }

  </script>
