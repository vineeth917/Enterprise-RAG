	<script>
		document.addEventListener('DOMContentLoaded', () => {
		const homeSearchBoxRoot = document.getElementById("coveo-homepage-searchbox");
		const headerSearchBoxRoot = document.getElementById("coveo-header-searchbox");
		const productSearchBoxRoot = document.getElementById("coveo-product-searchbox");
		const articleSearchBoxRoot = document.getElementById("coveo-article-searchbox");
		const docSearchBoxRoot = document.getElementById("coveo-doc-searchbox");
		// 2. Configure a search endpoint
		Coveo.SearchEndpoint.configureCloudV2Endpoint(
			'preciselynonproduction1nswxlmr1',
			'xx573f8ea1-9d40-49ed-a10f-5a87ee20dff2'
		);
		// 3. Initialize the Searchbox components 
		
		// When migrating to PROD the redirect Url has to be set to whatever is the URL of the global search page in Prod
		const redirectUrl = "https://customer.precisely.com/s/global-search/%40uri"	
		let preselectedFacet = window.location.href.includes("/products/")? `#t=All&f:@commonproduct=[${document.querySelector("h1.product-name").innerHTML.replace(" ", "%20")}]` :""
		console.log(preselectedFacet)
		
		homeSearchBoxRoot && Coveo.initSearchbox(
		  homeSearchBoxRoot,
		  redirectUrl
		);
			
		headerSearchBoxRoot && Coveo.initSearchbox(
		  headerSearchBoxRoot,
		  redirectUrl+preselectedFacet
		);	
		
		productSearchBoxRoot && Coveo.initSearchbox(
		  productSearchBoxRoot,
		  redirectUrl+preselectedFacet
		);
		
		articleSearchBoxRoot && Coveo.initSearchbox(
		  articleSearchBoxRoot,
		  redirectUrl+preselectedFacet+"&f:@commonsource=[Knowledge]"
		);	
		
		docSearchBoxRoot && Coveo.initSearchbox(
		  docSearchBoxRoot,
		  redirectUrl+preselectedFacet+"&f:@commonsource=[Documentation]"
		);
	})
	</script>

