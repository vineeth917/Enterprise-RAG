    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];

    (function()
    {
        var gads = document.createElement('script');
        gads.async = true;
        gads.type = 'text/javascript';
        var useSSL = 'https:' == document.location.protocol;
        gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(gads, node);
    })();

    var slots = {"ads-billboard":{"size_group":"billboard","slot_sizes":[[970,90],[320,100]],"unit_path":"\/1582411174\/bb_homepage","targetting":[]}};
    var size_groups = {"pc_article_right_sg":[{"viewport_size":[728,300],"slot_size":[[160,600],[120,600]]},{"viewport_size":[0,0],"slot_size":[300,250]}],"billboard":[{"viewport_size":[1000,0],"slot_size":[970,90]},{"viewport_size":[0,0],"slot_size":[320,100]}],"leaderboard_large":[{"viewport_size":[1000,0],"slot_size":[728,90]},{"viewport_size":[0,0],"slot_size":[300,250]}],"leaderboard_small":[{"viewport_size":[1000,0],"slot_size":[728,90]},{"viewport_size":[0,0],"slot_size":[320,100]}],"skyscraper":[{"viewport_size":[768,0],"slot_size":[160,600]},{"viewport_size":[0,0],"slot_size":[300,250]}]};

    //push ads in
    var sizes = {};
    googletag.cmd.push(function(){
        for(i in size_groups) {
            sizes[i] = googletag.sizeMapping();
            for(j in size_groups[i]) {
                sizes[i] = sizes[i].addSize(size_groups[i][j]['viewport_size'], size_groups[i][j]['slot_size']);
            }
            sizes[i] = sizes[i].build();
        }

        pushAds(slots);

        googletag.pubads().setTargeting('env', app.env);
        googletag.pubads().setTargeting('site', app.site);
        googletag.pubads().setTargeting('language', app.lang);
        googletag.pubads().setTargeting('pageType', app.pageType);
        googletag.pubads().setTargeting('section', app.section);
        googletag.pubads().setTargeting('entry', app.entry);
        googletag.pubads().setTargeting('page', app.page);
        googletag.pubads().setTargeting('subscriber', app.subscriber);

        googletag.pubads().enableSingleRequest();
        googletag.pubads().collapseEmptyDivs();
        googletag.enableServices();
    });

    function pushAds(slots) {
        for(i in slots) {
            if(slots[i]['size_group'] && !slots[i]['slot_sizes']) {
                slots[i]['slot_sizes'] = [];

                for(j in size_groups[ slots[i]['size_group'] ]) {
                    slots[i]['slot_sizes'].push(size_groups[ slots[i]['size_group'] ][j]);
                }
            }

            if(!slots[i]['size_group'] && !slots[i]['slot_sizes']) {
                var gt = googletag.defineOutOfPageSlot(slots[i]['unit_path'], i);
            }
            else {
                var gt = googletag.defineSlot(slots[i]['unit_path'], slots[i]['slot_sizes'], i);
            }

            for(j in slots[i]['targetting']) {
                gt.setTargeting(j, slots[i]['targetting'][j]);
            }

            if(slots[i]['size_group']) {
                gt.defineSizeMapping( sizes[ slots[i]['size_group'] ] );
            }

            gt.addService(googletag.pubads());
        }
    }

