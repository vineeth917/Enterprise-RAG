let customScrollToCheck = setInterval(function() {
if (window.jQuery) {
    $(document).on('click', '[data-scrollto], [data-tab-target]', (e) => {
        let target_id = e.currentTarget.dataset.scrollto || e.currentTarget.dataset.tabTarget;
        let target = document.getElementById(target_id);
        let windowCurrentWidth = window.innerWidth;
        if(target) { e.preventDefault(); }

        if(e.currentTarget.dataset.tabTarget && target) {
            target.click();
            //setTimeout(target.click(), 1000);
        }

        if(target) {
            let header = document.querySelector('body > header');
            let headerBounds = header.getBoundingClientRect();
            let targetBounds = target.getBoundingClientRect();
            let scrollTop = document.documentElement.scrollTop;
            let offset = 32;

            window.scrollTo({ top: targetBounds.top + scrollTop - headerBounds.height - offset, behavior: 'smooth' });

            // If data-scrollto
            //if(e.currentTarget.dataset.scrollto) {
            //    window.scrollTo({ top: targetBounds.top + scrollTop - headerBounds.height - offset, behavior: 'smooth' });
            //}

            // If data-tab-target, Scroll to destination only on tablet and desktop screensizes
            //if(windowCurrentWidth >= 769 && e.currentTarget.dataset.tabTarget) {
            //    window.scrollTo({ top: targetBounds.top + scrollTop - headerBounds.height - offset, behavior: 'smooth' });
            //}
        }
    })
  clearInterval(customScrollToCheck);
}
}, 100);
    

