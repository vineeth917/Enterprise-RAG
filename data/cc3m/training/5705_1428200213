} </style> <script id="fl-theme-custom-js">document.addEventListener("DOMContentLoaded", function() {
 var prevScrollPos = window.pageYOffset;
 var loadedProjects = 9;
 var allProjects = document.querySelectorAll('.project'); // Ensure this selector matches your project items
 // Function to load more projects
 function loadProjects(start, count) {
 var end = start + count;
 for (let i = start; i < end && i < allProjects.length; i++) {
 allProjects[i].style.display = 'block'; // Adjust this as needed for your site's styling
 }
 return Math.min(end, allProjects.length);
 }
 // Initially display 9 projects
 loadedProjects = loadProjects(0, loadedProjects);
 window.addEventListener("scroll", function() {
 var currentScrollPos = window.pageYOffset;
 var masthead = document.getElementById("masthead");
 // Masthead show/hide based on scrolling
 if (prevScrollPos > currentScrollPos) {
 masthead.style.transform = "translateY(0)";
 masthead.className = currentScrollPos === 0 ? "orgin_postion" : "mid_page";
 } else {
 masthead.style.transform = "translateY(-100%)";
 masthead.classList.remove("orgin_postion");
 masthead.classList.add("mid_page");
 }
 // Lazy load projects when reaching the bottom of the page
 if (window.innerHeight + window.pageYOffset >= document.body.offsetHeight - 1) {
 loadedProjects = loadProjects(loadedProjects, 6); // Load more projects
 }
 prevScrollPos = currentScrollPos;
 });
});  jQuery(document).ready(function( $ ) {
 $('.iframe-link').magnificPopup({
 type:'iframe'
 });
