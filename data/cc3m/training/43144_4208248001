<script type="text/javascript">

    let thisPageUrl = document.location.href;
    let thisTitle = document.title;
    let thisTwitterHome = $(".floatingSocialShare__ButtonTwitter").attr("data-home");

    const facebookSharerLink = "https://www.facebook.com/sharer/sharer.php?u=" + encodeURI(thisPageUrl);
    const twitterSharerLink = "https://twitter.com/intent/tweet?url=" + encodeURI(thisPageUrl) + "&text=" + encodeURI(" | " + thisTitle + " - " + thisTwitterHome);
    const linkedinSharerLink = "https://www.linkedin.com/shareArticle?mini=true&url=" + encodeURI(thisPageUrl);
    const whatsappSharerLink = "https://api.whatsapp.com/send?text=" + encodeURI(" | " + thisTitle + ": " + thisPageUrl);

    $(".floatingSocialShare__ButtonFacebook a").attr("href", facebookSharerLink);
    $(".floatingSocialShare__ButtonTwitter a").attr("href", twitterSharerLink);
    $(".floatingSocialShare__ButtonLinkedin a").attr("href", linkedinSharerLink);
    $(".floatingSocialShare__ButtonWhatsapp a").attr("href", whatsappSharerLink);
    
    $(document).ready(function(){
        $(".global-footer__nav").addClass("global-footer__nav--floatingSocialShare");
    })
    

    const popupOpener = ({ url, title, w, h }) => {
        // Fixes dual-screen position                             Most browsers      Firefox
        const dualScreenLeft = window.screenLeft !== undefined ? window.screenLeft : window.screenX;
        const dualScreenTop = window.screenTop !== undefined ? window.screenTop : window.screenY;

        const width = window.innerWidth ? window.innerWidth : document.documentElement.clientWidth ? document.documentElement.clientWidth : screen.width;
        const height = window.innerHeight ? window.innerHeight : document.documentElement.clientHeight ? document.documentElement.clientHeight : screen.height;

        const systemZoom = width / window.screen.availWidth;
        const left = (width - w) / 2 / systemZoom + dualScreenLeft
        const top = (height - h) / 2 / systemZoom + dualScreenTop
        const newWindow = window.open(url, title,
            `
        scrollbars=yes,
        width=${w / systemZoom},
        height=${h / systemZoom},
        top=${top},
        left=${left}
        `
        )

        if (window.focus) newWindow.focus();
    }

</script> </main> <!-- Start Footer --> <footer class="global-footer"> <div class="global-footer__actions"> <div class="global-footer__actions__wrapper"> <div class="global-footer__social"> 

