<script>
  var cTime;
  var timeOfDayNow = "evening";
  var greetingNow = "Good Evening";

  function getCurrentTime() {
    const dateOptions = { timeZone: "America/New_York" };
    const cDate = new Date();
    const timeOnly = cDate.toLocaleTimeString('en-US', dateOptions);
    const currTime = timeOnly.split(':');
    const h = currTime[0];
    const m = currTime[1];
    const ampm = currTime[2].split(' ')[1];
    const displayTime = h + ':' + m + ' ' + ampm;
    cTime = {
      displayTime: displayTime,
      greetingLabel: greetingNow,
      hour: h,
      hrId: ampm,
      bannerId: timeOfDayNow
    };
    $('.current-bh-time')[0].textContent = cTime.displayTime;
  }
</script>

    <script>
        function mobileTapping(htmlElement) {
            const element = $(htmlElement);
            const url = element.data('url');
            setTimeout(function() {
                element.children('a').eq(0).attr('href',url);
            }, 50);
        }
    </script>

<script>
    getCurrentTime();
    setInterval(getCurrentTime, 30000);

    $(".letter-split").text(cTime.greetingLabel);

    if (cTime.bannerId === 'morning') {
        $('.homebanner').css('background-image', 'url(https://www.biltmore.com/wp-content/uploads/2025/04/V2TallBanner_Spring_Morning_1900x750.jpg)');
    }
    if (cTime.bannerId === 'afternoon') {
        $('.homebanner').css('background-image', 'url(https://www.biltmore.com/wp-content/uploads/2025/04/V2TallBanner_Spring_Daytime_1900x745.jpg)');
    }
    if (cTime.bannerId === 'evening') {
        $('.homebanner').css('background-image', 'url(https://www.biltmore.com/wp-content/uploads/2025/04/V2TallBanner_Spring_Evening_1900x745.jpg)');
    }

    updateBannerImg();

    function updateBannerImg() {
        if (window.innerWidth > 360 && window.innerWidth < 640) {
            const text = $(".letter-split").text();
            $(".letter-split").empty();
            $(".letter-split").append($("<span class='split-word'>").text(text));
            $(".split-word").html(function (index, html) {
                return html.replace(/./g, function (char) {
                    return char === " " ? '<span>&nbsp;</span>' : `<span>${char}</span>`;
                });
            });
        } else {
            var words = $(".letter-split").text().split(" ");
            $(".letter-split").empty();
            $.each(words, function (i, v) {
                $(".letter-split").append($("<span class='split-word'>").text(v));
            });

            $(".split-word").html(function (index, html) {
                return html.replace(/\S/g, '<span>$&</span>');
            });
        }
    }

    // Preload buttons for each section (if exists)
    const bannerButtons = [
        "<div class=\"home-banner-buttons\"><a href=\"\/visit\/biltmore-estate\/biltmore-house\/?utm_source=main_banner&#038;utm_medium=bilt-homepage&#038;utm_campaign=april-2025-House\"\n                            class=\"btn gold gold-button-nav outlined\"\n                            target=\"\">Biltmore House<\/a><\/div>",
        "<div class=\"home-banner-buttons\"><a href=\"\/visit\/biltmore-estate\/gardens-grounds\/?utm_source=main_banner&#038;utm_medium=bilt-homepage&#038;utm_campaign=april-2025-Gardens\"\n                            class=\"btn blue gold-button-nav outlined\"\n                            target=\"\">Gardens &amp; Grounds<\/a><\/div>",
        "<div class=\"home-banner-buttons\"><a href=\"\/visit\/winery\/?utm_source=main_banner&#038;utm_medium=bilt-homepage&#038;utm_campaign=april-2025-Winery\"\n                            class=\"btn wine gold-button-nav outlined\"\n                            target=\"\">Biltmore Winery<\/a><\/div>",
        "<div class=\"home-banner-buttons\"><a href=\"\/visit\/biltmore-estate\/antler-hill-village\/?utm_source=main_banner&#038;utm_medium=bilt-homepage&#038;utm_campaign=april-2025-Village%20\"\n                            class=\"btn white gold-button-nav outlined\"\n                            target=\"\">Antler Hill Village<\/a><\/div>"    ];

    $(function() {
        function handleHover(child) {
            $('.mb-loc').addClass('hide-for-small-only')
            $('.home-banner-buttons-wrapper').html(bannerButtons[child - 1]);
            anime.remove('.mb-loc');
            anime.remove('.home-banner-buttons');
            $('.hb-right-ul li').removeClass('hb-active');
            $('.hb-right-ul li:nth-child(' + child + ')').addClass('hb-active');
            var words = $(".letter-split").text().split(" ");
            $(".letter-split").empty();
            $.each(words, function (i, v) {
                $(".letter-split").append($("<span class='split-word'>").text(v));
            });
            $(".split-word").html(function (index, html) {
                return html.replace(/\S/g, '<span>$&</span>');
            });
            anime({
                targets: '.split-word span',
                translateY: [45, 0],
                translateZ: 0,
                opacity: [0, 1],
                easing: "easeInOutQuad",
                duration: 800,
                direction: 'forwards',
                delay: function (el, i) {
                    return 25 * (i + 1)
                }
            });
            anime({
                targets: '.mb-loc',
                translateY: [25, 0],
                translateZ: 0,
                opacity: [0, 1],
                easing: "easeInOutQuad",
                duration: 800,
                delay: 400,
                direction: 'forwards',
            });
            anime({
                targets: '.mb-sub',
                translateY: [25, 0],
                translateZ: 0,
                opacity: [0, 1],
                easing: "easeInOutQuad",
                duration: 800,
                direction: 'forwards',
            }, '-=800');
            anime({
                targets: '.home-banner-buttons',
                translateY: [25, 0],
                translateZ: 0,
                opacity: [0, 1],
                easing: "easeInOutQuad",
                duration: 800,
                delay: 550,
                direction: 'forwards',
            });
        }

        $('.hb-right-ul li').hoverIntent(function (e) {
            $('.hb-right-ul.hide-for-medium li > a.dbl-tap').removeAttr('href');
            $(this).parent('.hb-right-ul.hide-for-medium').length && mobileTapping(this);
            $('.mb-time').hide();
            $('.mb-sub').show();
            $('.blackfade').css('opacity', '1');
            if($(this).is(':nth-child(1)') && !$('.hb-right-ul li:nth-child(1)').hasClass('hb-active')) {
                $('.hb-vid').empty();
                if ($('.hb-vid').css('display') == 'block') {
                    $('.hb-vid').load('/wp-content/themes/FoundationPress/modules/vids/Vid01.php');
                }
                $('.homebanner').css('background-image', 'url(https://www.biltmore.com/wp-content/uploads/2019/11/MOptHouse1.jpg)');
                $('.mb-sub').text('Discover');
                $('.letter-split').html('Renowned Architecture');
                $('.mb-loc').html('Visit the Vanderbilts&#039; Magnificent Family Home');
                handleHover(1);
            }
            if($(this).is(':nth-child(2)') && !$('.hb-right-ul li:nth-child(2)').hasClass('hb-active')) {
                $('.hb-vid').empty();
                if ($('.hb-vid').css('display') == 'block') {
                    $('.hb-vid').load('/wp-content/themes/FoundationPress/modules/vids/Vid02.php');
                }
                $('.homebanner').css('background-image', 'url(https://www.biltmore.com/wp-content/uploads/2019/11/MOptGardens1.jpg)');
                $('.mb-sub').text('Explore');
                $('.letter-split').html('Artful Landscapes');
                $('.mb-loc').html('Stroll Stunning Gardens Designed by Frederick Law Olmsted');
                handleHover(2);
            }
            if($(this).is(':nth-child(3)') && !$('.hb-right-ul li:nth-child(3)').hasClass('hb-active')) {
                $('.hb-vid').empty();
                if ($('.hb-vid').css('display') == 'block') {
                    $('.hb-vid').load('/wp-content/themes/FoundationPress/modules/vids/Vid03.php');
                }
                $('.homebanner').css('background-image', 'url(https://www.biltmore.com/wp-content/uploads/2019/11/Winery1.jpg)');
                $('.mb-sub').text('Enjoy');
                $('.letter-split').html('Award-Winning Wines');
                $('.mb-loc').html('Linger at Our Relaxing Winery');
                handleHover(3);
            }
            if($(this).is(':nth-child(4)') && !$('.hb-right-ul li:nth-child(4)').hasClass('hb-active')) {
                $('.hb-vid').empty();
                if ($('.hb-vid').css('display') == 'block') {
                    $('.hb-vid').load('/wp-content/themes/FoundationPress/modules/vids/Vid04.php');
                }
                $('.homebanner').css('background-image', 'url(https://www.biltmore.com/wp-content/uploads/2019/11/MOptAntler-Hill-11.jpg)');
                $('.mb-sub').text('Experience');
                $('.letter-split').html('Casual Entertainment');
                $('.mb-loc').html('Unwind with Live Music, Dining, and Shopping');
                handleHover(4);
            }
        });
    });
</script>
