<script>
	require([
		'plugins_collections_custom_collection_default_slider_package',
		'plugins_common_custom_tripbuilder/site_tripbuilder'
	], function(sliderPackage, tripbuilder) {
		const root = document.querySelector('[data-guid="55b1948a-3be5-4457-850a-e3da93d07f57"]');
		let headerWithArrows;
		let headerArrows;
		
		sliderPackage.init(`55b1948a-3be5-4457-850a-e3da93d07f57`, slider => {
			if (!slider) return;

			headerWithArrows = root.querySelector('[data-header-with-arrows]');
			headerArrows = Array.from(root.querySelectorAll('[data-prev-arrow], [data-next-arrow]'));

			if (headerWithArrows && headerArrows.length) {
				headerArrows.forEach((arrow, index) => {
					const go = index === 0 ? '<' : '>';

					arrow.addEventListener('click', () => slider.go(go));
				});
			}

			const tbButtons = Array.from(root.querySelectorAll('[data-tb-button]'));
			
			if (!tbButtons.length) return;

			tripbuilder.initButtons({
				root,
				addButtonTemplate : `<button data-tb-add class="tripbuilder-button add" aria-label="Add to Trip"></button>`,
				removeButtonTemplate : `<button data-tb-remove class="tripbuilder-button remove" aria-label="Remove from Trip"></button>`
			});

			tripbuilder.updateButtons(root);
		});
	});
</script>
</div><div data-civid="67fd175f91b1c1795610d7ae" class="contentRender contentRender_18 contentRender_type_widget contentRender_name_plugins_collections_type_custom_slides contentRender_name_plugins_collections_template_custom_core_v2_side_by_side_3_across" data-sv-time="353" data-sv-index="18" data-gtm-vars='{ "tCore" : { "contentNum" : "2" } }'>













