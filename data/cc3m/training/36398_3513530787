<script>
	require([
		'plugins_nav_custom_nav_desktop_dropdown',
		'plugins_nav_custom_nav_mobile',
		'plugins_common_custom_utils'
	], function(
		desktopNav,
		mobileNav,
		utils
	) {
		const root = document.querySelector('[data-guid="88c9f110-83e4-4d4a-afac-e7a3db6f2218"]');
		const els = {
			ariaHide: Array.from(document.querySelectorAll('[data-aria-hide], #quickview-detail')),
			bodyOverlay: document.querySelector('[data-body-overlay="88c9f110-83e4-4d4a-afac-e7a3db6f2218"]'),
			rootPanel: document.querySelector('.root-panel'),
			menuToggle: root.querySelector('[data-action="toggle-menu"]'),
			mobileMenu: root.querySelector('[data-mobile-menu]'),
		};

		// Ensure there is actually a navigation widget instead of Quick Links before initializing them below
		const navChecks = {
			mainNav: root.querySelector('[data-main-nav] [data-nav-check]'),
			secondaryNav: root.querySelector('[data-secondary-nav] [data-nav-check]'),
			mobileNav: root.querySelector('[data-mobile-navs] [data-nav-check]'),
		};

		const focusables = 'a, button, input, select, textarea, iframe, area, details, progress';
		const breakpoint = 1024;
		let mobileNavOpen = false;

		function handleMobileMenuToggle() {
			if (mobileNavOpen) closeMobileMenu();
			else openMobileMenu();
		}

		function handleResize() {
			if (window.innerWidth >= breakpoint && mobileNavOpen) closeMobileMenu();
		}

		function openMobileMenu() {
			[els.bodyOverlay, els.menuToggle, els.mobileMenu].forEach((el) => el.classList.add('active'));
			ariaToggle();
			mobileNavOpen = true;
		}

		function closeMobileMenu() {
			[els.bodyOverlay, els.menuToggle, els.mobileMenu].forEach((el) => el.classList.remove('active'));
			ariaToggle(false);
			mobileNavOpen = false;
		}

		function ariaToggle(hide = true) {
			els.menuToggle.setAttribute('aria-expanded', hide);

			els.ariaHide.forEach((el) => {
				const focus = Array.from(el.querySelectorAll(focusables));
				
				if (hide) {
					el.setAttribute('aria-hidden', true);

					focus.forEach((f) => {
						if (f.hasAttribute('tabindex')) f.setAttribute('data-tabindex', f.getAttribute('tabindex'));

						f.setAttribute('tabindex', -1);
					});
				}
				else {
					el.removeAttribute('aria-hidden');

					focus.forEach((f) => {
						if (f.hasAttribute('data-tabindex')) f.setAttribute('tabindex', f.getAttribute('data-tabindex'));
						else f.removeAttribute('tabindex');
					});
				}
			});
		}

		function bindEventListeners() {
			els.menuToggle?.addEventListener('click', handleMobileMenuToggle);
			els.bodyOverlay.addEventListener('click', closeMobileMenu);
			['resize', 'orientationchange'].forEach((ev) => window.addEventListener(ev, utils.debounce(handleResize, 200)));
		}

		function init() {
			bindEventListeners();

			
				if(!!navChecks.mobileNav) {
					mobileNav.init({
						root,
						selector: '[data-mobile-navs]',
						depth: 3,
					});
				}
			

			
				if(!!navChecks.secondaryNav) {
					desktopNav.init({
						root,
						selector: '[data-secondary-nav]',
						depth: 1,
					});
				}
			

			
				if(!!navChecks.mainNav) {
					desktopNav.init({
						root,
						selector: '[data-main-nav]',
						depth: 3,
					});
				}
			

			// Adding themes to root panel for body updates
			
			const theme = "default";
			
			if (theme) els.rootPanel.classList.add(theme, `${theme}-theme`);
		}

		//=====----- INIT -----=====//

		init();
	});
</script></div>
	</div>
	<div id="slideshow-container" class="slideshow-container" data-aria-hide>
		<div data-civid="67fd175f91b1c1795610d7a4" class="contentRender contentRender_11 contentRender_type_widget contentRender_name_plugins_collections_type_custom_header_slides contentRender_name_plugins_collections_template_custom_core_v2_hero_slideshow" data-sv-time="348" data-sv-index="11" data-gtm-vars='{ "tCore" : { "contentNum" : "1" } }'>









