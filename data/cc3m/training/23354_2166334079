<script type="text/javascript">
jQuery(document).ready(function(){
    jQuery(window).scroll(function(){ 
        if(jQuery(this).scrollTop() < 50){
            jQuery("header").removeClass("header-compact");
        }
        else{
            jQuery("header").addClass("header-compact");
        }
    });
});
</script>


