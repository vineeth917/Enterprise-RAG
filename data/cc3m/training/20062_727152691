<script>
    function onSubmit(token) {
        var e = $("input#orderid").val();
        var m = $("input#email").val();
        $.ajax({
            type: "POST",
            url: "https://buy.fans/ajax/checkorder/",
            data: {
                id: e,
                email: m,
                action: "checkorder"
            },
            success: function (e) {
                1 == e.status ? window.location.replace("/checkorder/" + e.hash + "/" + e.email) : Swal.fire("Üzgünüm!", e.msg, "error")
            }
        })
    }
</script>
