        font-size: 24px;
        }

    
                body #primary .em-widget-subtitle {
        font-size: 14px;
        }

    
                body #primary .widget .article-title{
        font-size: calc(18px - 2px);
        }

        body #primary .widget .article-title.article-title-2,
        .em-latest-post-carousel .slider-figcaption .slide-title-1{
        font-size: 18px;
        }
        }
