      <script type="text/javascript">
        let breakingBannerExists = document.querySelectorAll('.Banner').length > 0;
        const observer = new ResizeObserver(entries => {
          entries.forEach(entry => {
            let sponsorAdContent = document.querySelector('.GoogleDfpAd-Sponsorship-Content');
            if (breakingBannerExists) {
              setSponsorStyle();
            }
            else {
              document.querySelector('.AdOneColumnContainer-container').style.marginTop = sponsorAdContent.offsetHeight+"px";
            }
            if (sponsorAdContent.offsetHeight > 5){
              document.querySelector('.GoogleDfpAd-advertisement').style.display = "block";
            }
            else{
              document.querySelector('.GoogleDfpAd-advertisement').style.display = "none";
            }
            document.querySelector('#rotating-widgets').style.top = sponsorAdContent.offsetHeight + document.querySelector('.Page-header').offsetHeight + 8 + "px";
          })
        })
        observer.observe(document.querySelector('.GoogleDfpAd-Sponsorship-Content'));

        if (breakingBannerExists) {
          window.addEventListener("scroll", setSponsorStyle);
        }
        function setSponsorStyle() {
          let sponsorAdContent = document.querySelector('.GoogleDfpAd-Sponsorship-Content');
          breakingBannerExists = document.querySelectorAll('.Banner').length > 0;

          if (document.querySelectorAll('.AdOneColumnContainer-container').length > 0) {
            if (breakingBannerExists && document.documentElement.scrollTop <= document.querySelector('.Banner').offsetHeight) {
                sponsorAdContent.style.position = "relative";
                sponsorAdContent.style.top = "0px";
                document.querySelector('.AdOneColumnContainer-container').style.marginTop = "0px";
            } else {
                sponsorAdContent.style.position = "fixed";
                sponsorAdContent.style.top = document.querySelector('.Page-header').offsetHeight+"px";
                document.querySelector('.AdOneColumnContainer-container').style.marginTop = sponsorAdContent.offsetHeight+"px";
            }
          }
        }
      </script>
    

    
