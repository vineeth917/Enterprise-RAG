<script>
  window.user = () => {}
  let navSettingsKey = null
  
  let auth0Domain = 'login.forumcomm.com'
  let debug = false
  

  let firstEntry = getForumCookie('_fcc_first_entry')
  const feDate = new Date()
  let feCurrentDate = feDate.toLocaleDateString("en-CA")
  let firstEntryCampaignActive = false

  function ensureUserIsSet() {
    return new Promise(waitForUser)

    function waitForUser(resolve) {
      if (window.user && window.user.status) resolve(window.user.status)
      else setTimeout(waitForUser.bind(this, resolve), 10)
    }
  }

  ;(function () {
    let wdayRegex = /(\.com\/wdayplus)/
    let checkMatch = window.location.href.match(wdayRegex)
    if (checkMatch) {
      navSettingsKey = 'liveStream'
    }
  })()

  ensureUserIsSet().then(function () {
    if (window.user.status !== getForumCookie('_fcc_user_status')) {
      setForumCookie('_fcc_user_status', window.user.status, 180)
      dataLayer.push({ FCC_UserStatus: window.user.status })
    }
  })

  function setLoginButton(displayButton) {
    try {
      ;(document.getElementById('MG2login') || {}).style.display = displayButton
    } catch (e) {
      console.log('no login button')
    }
  }

  function showSubscribeButtonBanner() {
    try {
      let subButtonItems = document.querySelectorAll('.Page-header-subscribe')
      subButtonItems.forEach((item) => {
        if (window.innerWidth < 768 && item.parentElement.className === 'Page-header') {
          item.style.display = 'flex'
          let firstEntryContainer = document.querySelector('.GoogleDfpAd-FirstEntryTakeover-Content > *:first-child > *:first-child > *:first-child')
          if (firstEntryContainer !== null) {
            let firstEntryIframe = firstEntryContainer.querySelectorAll('iframe').length
            if (firstEntryIframe > 0) {
              firstEntryCampaignActive = true
            }
          }
          if (document.querySelector('.Page-header-hat')) {
            if (firstEntryCampaignActive) {
              // dont add top margin
            } else if (document.querySelector('#agweek bsp-banner')) {
              document.querySelector('bsp-banner').style.marginTop = '40px'
            } else {
              document.querySelector('.Page-header-hat').style.marginTop = '40px'
            }
          } else if (document.querySelector('.Page-above')) {
            if (firstEntryCampaignActive) {
              // dont add top margin
            } else if (document.querySelector('bsp-banner')) {
              document.querySelector('bsp-banner').style.marginTop = '40px'
            } else {
              document.querySelector('.Page-above').style.marginTop = '40px'
            }
          } else if (document.querySelector('.account-page-container')) {
            document.querySelector('.account-page-container').style.marginTop = '40px'
          } else {
            document.querySelector('.Page-content').style.marginTop = '40px'
          }
        } else if (window.innerWidth >= 768 && item.parentElement.className === 'Page-header-bar') {
          item.style.display = 'flex'
          document.getElementById('MG2Subscribe').style.display = 'flex'
        }
      })
    } catch (e) {
      console.log('no subscribe button')
    }
  }

  function hideSubscribeButtonBanner() {
    try {
      ;(document.getElementById('MG2Subscribe') || {}).style.display = 'none'
    } catch (e) {
      console.log('no login button')
    }
  }

  function setAccountButton(displayButton) {
    try {
      ;(document.getElementById('MG2MyAccount') || {}).style.display = displayButton
    } catch (e) {
      console.log('no account button')
    }
  }

  MG2Loader.init({
    plugins: [
      {
        name: 'FP',
        initOptions: {
          version: 'forum',
          environment: 'prod',
        },
      },
      {
        name: 'DL',
        initOptions: {
          version: 'forum',
          collectors: ['connext'],
          tagManager: 'GTM',
          containerId: 'GTM-P7XVL79',
        },
      },
      {
        name: 'NXT',
        initOptions: {
          clientCode: 'forum',
          environment: 'prod',
          siteCode: 'TF',
          configCode: 'LAUNCH1',
          settingsKey: navSettingsKey,
          debug: debug,
          authSettings: {
            domain: auth0Domain,
            client_id: 'Gwag9XjtaUoW5A9KZDIPfZqXYi90TX2V',
            redirect_uri: window.location.origin + '/redirect',
            useRefreshTokens: true,
            scope: 'openid email profile offline_access',
          },
          publicEventHandlers: {
            onNotAuthorized: function (eventData) {
              //event fires on every page load if visitor is logged out
              window.user.status = 'anonymous'
              setLoginButton("flex")
              
                showSubscribeButtonBanner()
              
              setAccountButton("none")
            },
            onAuthorized: function (eventData) {
              //event fires on every page load if visitor is logged in but has access to this product
              window.user.status = 'registered'
              window.user.email = eventData.MG2AccountData.Email
              window.user.auth0Id = eventData.MG2AccountData.MasterId
              setLoginButton("none")
              
                showSubscribeButtonBanner()
              
              setAccountButton("flex")
            },
            onHasNoActiveSubscription: function (eventData) {
              //event fires on every page load if visitor is logged in but their subscription is stopped or inactive
              window.user.status = 'registered'
              window.user.email = eventData.MG2AccountData.Email
              window.user.auth0Id = eventData.MG2AccountData.MasterId
              setLoginButton("none")
              
                showSubscribeButtonBanner()
              
              setAccountButton("flex")
            },
            onHasAccessNotEntitled: function (eventData) {
              //event fires on every page load if visitor is logged in and has no access to this product, but has access to other products)
              window.user.status = 'registered'
              window.user.email = eventData.MG2AccountData.Email
              window.user.auth0Id = eventData.MG2AccountData.MasterId
              setLoginButton("none")
              
                showSubscribeButtonBanner()
              
              setAccountButton("flex")
            },
            onHasAccess: function (eventData) {
              //event fires on every page load if visitor is subscriber
              window.user.status = 'subscribed'
              window.user.email = eventData.MG2AccountData.Email
              window.user.auth0Id = eventData.MG2AccountData.MasterId
              setLoginButton("none")
              hideSubscribeButtonBanner()
              setAccountButton("flex")
            },
          },
        },
      },
    ],
  })
</script>
    

    
       
       
            <script>
    if (typeof window.blueConicClient !== 'undefined' &&
        typeof window.blueConicClient.event !== 'undefined' &&
        typeof window.blueConicClient.event.subscribe !== 'undefined') {
            ensureUserIsSet().then(() => {
                if (user.status != "anonymous") {
                    let blueProfile = blueConicClient.profile.getProfile();
                    if (!checkConsentGDPR(blueProfile)) {
                        addAuth0Info(blueProfile)
                    }
                }
            })
    } else {
        window.addEventListener('onBlueConicLoaded', function () {
            ensureUserIsSet().then(() => {
                if (user.status != "anonymous") {
                    let blueProfile = blueConicClient.profile.getProfile();
                    if (!checkConsentGDPR(blueProfile)) {
                        addAuth0Info(blueProfile)
                    }
                }
            })
        }, false);
    }
</script>
        
    

    
    
  
    <script type="text/javascript">
      /**
       * Function to handle Viafoura login events
       * @param {boolean} isInit If handleLogin is being called on the initial page load
       */
      const handleLogin = async (isInit) => {
        ensureUserIsSet().then(async () => {
          if (user.status === 'anonymous') {
            if (!isInit) {
              // Should not redirect on page load
              document.getElementById('MG2login').click()
            }
          } else {
            const idToken = await getTokens()
            if (idToken !== '' && idToken !== undefined && idToken !== null) {
              const response =
                await window.vf.session.login.openIdConnect(idToken)

              if (response !== '' && response !== undefined && response !== null) {
                console.error('Viafoura OICD error: ' + JSON.stringify(response))
                handleLogin(false) // ! make sure this recursion doesn't break everything
              } else {
                console.log('Viafoura: Login successful!')
              }
            } else {
              console.error('Viafoura: Could not obtain ID token!')
            }
          }
        })
      }

      /**
       * Attempts to get the ID and Refresh tokens from localstorage or AWS.
       * @returns {Promise} An ID token or an empty string if one cannot be obtained.
       */
      const getTokens = async () => {
        try {
          // First, try to use cookies
          let idToken = getForumCookie('auth0_jwt')
          if (idToken !== '' && idToken !== undefined) {
            return idToken
          } else {
            idToken = ''
          }

          // Second, try to use localstorage
          let expires = ''
          const localStorageTokens = await getLocalStorageTokens()
          if (localStorageTokens !== '') {
            idToken = localStorageTokens.idToken
            refreshToken = localStorageTokens.refreshToken
            if (idToken !== undefined && idToken !== '') {
              return idToken
            }
          }
          return idToken
        } catch (error) {
          console.error('Viafoura: ' + error)
          return ''
        }
      }

      /**
       * Gets the ID and/or refresh token(s) from localstorage. Note that only the refresh token might be returned.
       * @returns {object} Object containing the id and/or refresh token(s) or an empty string if no tokens exist.
       */
      const getLocalStorageTokens = async () => {
        const localStorageString = await getLocalStorageString()
        const tokensObject = {}
        if (localStorageString === null || localStorageString === '') {
          console.log('Viafoura: Auth0 tokens not found in localstorage.')
          return ''
        }

        const localStorageObject = JSON.parse(localStorageString)
        const idToken = localStorageObject.body.id_token
        const refreshToken = localStorageObject.body.refresh_token
        const idTokenExpiration = localStorageObject.expiresAt

        if (refreshToken !== undefined && refreshToken !== '') {
          tokensObject.refreshToken = refreshToken
        } else {
          tokensObject.refreshToken = ''
        }

        if (idToken !== undefined && idToken !== '') {
          tokensObject.idToken = idToken
        } else {
          tokensObject.idToken = ''
        }

        if (idTokenExpiration !== '' && idTokenExpiration !== undefined) {
          tokensObject.expires = idTokenExpiration
        } else {
          tokensObject.expires = ''
        }

        return tokensObject
      }

      /**
       * Get the Auth0 SPA JS string from localstorage
       * @returns {Promise} A localstorage string if it exists or an empty string if does not
       */
      const getLocalStorageString = async () => {
        const config = window.location.origin + '/_auth.config.json'
        const publication = getPublicationInfo()
        const response = await fetch(config)
        const json = await response.json()
        const clientId =
          json[publication.environment][publication.name].clientId
        const key =
          '@@auth0spajs@@::' +
          clientId +
          '::default::openid profile email offline_access'
        return localStorage.getItem(key)
      }

      /**
       * Get the publication and environment
       * @returns {object} An object containing the publication and environment
       */
      const getPublicationInfo = () => {
        const domain = window.location.hostname.split('.')
        let name, environment
        if (
          domain.includes('lower') ||
          domain.includes('localhost') ||
          domain.includes('127')
        ) {
          name = domain[0]
          environment = 'test'
        } else {
          if (!domain.includes('www')) {
            name = domain[0]
          } else {
            name = domain[1]
          }
          environment = 'production'
        }
        return { name, environment }
      }

      /**
       * Function for handling banned users.
       * Will not display an alert for the first login failure event.
       * @param {object} errorContext
       * @param {boolean} skipFirstAlert
       */
      const handleBannedUser = (errorContext, skipFirstAlert) => {
        const fccViafouraBanned = getForumCookie('_fcc_viafoura_banned')
        ensureUserIsSet().then(() => {
          if (fccViafouraBanned == null || user.status === 'anonymous') {
            setForumCookie('_fcc_viafoura_banned', 'false', 0)
          }

          if (
            errorContext.reason !== null &&
            errorContext.reason !== undefined &&
            errorContext.reason !== ''
          ) {
            console.log('Viafoura: ' + errorContext.reason)
          }

          if (errorContext.error_key === 'login_banned') {
            if (skipFirstAlert === false) {
              alert(
                'Your access to these tools has been restricted as a result of not adhering to the Commenting Policy available on the Policies and Standards page.'
              )
            }
            if (fccViafouraBanned === 'false') {
              setForumCookie('_fcc_viafoura_banned', 'init', 0)
            }
            return false
          }
        })
      }

      /**
       *  Function for handling the read comments event.
       *  Display an alert if the user is banned.
       */
      const handleReadComments = () => {
        if (getForumCookie('_fcc_viafoura_banned') === 'init') {
          alert(
            'Your access to these tools has been restricted as a result of not adhering to the Commenting Policy available on the Policies and Standards page.'
          )
          setForumCookie('_fcc_viafoura_banned', 'true', 0)
        }
      }

      window.vfQ = window.vfQ || []
      window.vfQ.push(async () => {
        // Logs in to Viafoura on page load
        handleLogin(true)

        // Intercepts default log in event
        window.vf.$prepublish((channel, event, ...args) => {
          if (channel === 'authentication' && event === 'required') {
            window.vf.$publish('tray', 'close')
            handleLogin(false)
            return false
          } else {
            return { channel, event, args }
          }
        })

        // Handles banned user messages
        window.vf.$subscribe('login', 'success', () => {
          setForumCookie('_fcc_viafoura_banned', 'false', 0)
        })

        let skipFirstAlert = true
        window.vf.$subscribe('login', 'failure', (errorContext) => {
          skipFirstAlert = handleBannedUser(errorContext, skipFirstAlert)
        })

        window.vf.$subscribe('comments', 'read', () => {
          handleReadComments()
        })
      })
    </script>
  


    
    
    
        <script type="text/javascript">
            /**
            * Dynamically generates an ad and inserts it into the template when the Viafoura conversations widget requests an ad.
            * @param  {String} slotName - id of the element to render the ad in
            */
            function generateCommentAd(slotName) {
              let viafouraAd = document.querySelector('[data-comments-ad-enable="true"]');

              // Generate next slot name
                var adUnitString = viafouraAd.getAttribute('data-slot-name') || '';
                var sizesAttribute = viafouraAd.getAttribute('data-slot-sizes');
                var sizesArray = JSON.parse(sizesAttribute);

                var sizeMappingString = viafouraAd.getAttribute('data-slot-adsizemap')
                var sizeMappingArray = JSON.parse(sizeMappingString);

                var adTargetingAttribute = viafouraAd.getAttribute('data-targeting')
                var adTargetingArray = JSON.parse(adTargetingAttribute);

                // Define the slot itself, call display() to
                // register the div and refresh() to fetch the ad.
                window.googletag.cmd.push(function() {
                    // Destroy any lingering ad slots if they exist in the widget
                    const slots = window.googletag.pubads().getSlots()
                        .filter((slot) => slot.getSlotElementId() === slotName);
                    window.googletag.destroySlots(slots);

                    // Define ad sizes based on viewport size
                    // NOTE ads are not responsive but new ads will be rendered to the appropriate size
                    // Adapted from https://support.google.com/dfp_premium/answer/4578089?hl=en#responsiveDesign
                    var mapping = googletag.sizeMapping();
                    sizeMappingArray.forEach(function (entry) {
                        var viewportSize = entry[0];
                        var adSizes = entry[1];
                        mapping.addSize(viewportSize, adSizes);
                    });

                    var sizeMapping = mapping.build();
                    // Create a new ad slot, pointing to the slotName given to this function
                    var slot = window.googletag.defineSlot(adUnitString, sizesArray, slotName)
                        .defineSizeMapping(sizeMapping)
                        .addService(window.googletag.pubads());

                    // Add any targeting or any other modifications to the ad here
                    if (adTargetingArray) {
                      for (let key in adTargetingArray) {
                        if (adTargetingArray.hasOwnProperty(key)) {
                          slot.setTargeting(key, adTargetingArray[key])
                        }
                      }
                    }

                    // Display the new ad
                    window.googletag.display(slotName);
                    window.googletag.pubads().refresh([slot]);
                });
            }

            /**
            * Dynamically generates an ad and inserts it into the template when the Viafoura liveblog widget requests an ad.
            * @param  {String} slotName - id of the element to render the ad in
            */
            function generateLiveblogAd(slotName) {
              let viafouraAd = document.querySelector('[data-blog-ad-enable="true"]');
              // Generate next slot name
                var adUnitString = viafouraAd.getAttribute('data-slot-name') || '';
                var sizesAttribute = viafouraAd.getAttribute('data-slot-sizes');
                var sizesArray = JSON.parse(sizesAttribute);

                var sizeMappingString = viafouraAd.getAttribute('data-slot-adsizemap')
                var sizeMappingArray = JSON.parse(sizeMappingString);

                var adTargetingAttribute = viafouraAd.getAttribute('data-targeting')
                var adTargetingArray = JSON.parse(adTargetingAttribute);

                // Define the slot itself, call display() to
                // register the div and refresh() to fetch the ad.
                window.googletag.cmd.push(function() {
                    // Destroy any lingering ad slots if they exist in the widget
                    const slots = window.googletag.pubads().getSlots()
                        .filter((slot) => slot.getSlotElementId() === slotName);
                    window.googletag.destroySlots(slots);

                    // Define ad sizes based on viewport size
                    // NOTE ads are not responsive but new ads will be rendered to the appropriate size
                    // Adapted from https://support.google.com/dfp_premium/answer/4578089?hl=en#responsiveDesign
                    var mapping = googletag.sizeMapping();
                    sizeMappingArray.forEach(function (entry) {
                        var viewportSize = entry[0];
                        var adSizes = entry[1];
                        mapping.addSize(viewportSize, adSizes);
                    });

                    var sizeMapping = mapping.build();
                    // Create a new ad slot, pointing to the slotName given to this function
                    var slot = window.googletag.defineSlot(adUnitString, sizesArray, slotName)
                        .defineSizeMapping(sizeMapping)
                        .addService(window.googletag.pubads());

                    // Add any targeting or any other modifications to the ad here
                    if (adTargetingArray) {
                      for (let key in adTargetingArray) {
                        if (adTargetingArray.hasOwnProperty(key)) {
                          slot.setTargeting(key, adTargetingArray[key])
                        }
                      }
                    }

                    // Display the new ad
                    window.googletag.display(slotName);
                    window.googletag.pubads().refresh([slot]);
                });
            }

            /**
            * Dynamically generates an ad and inserts it into the template when the Viafoura trending conversations widget requests an ad.
            * @param  {String} slotName - id of the element to render the ad in
            */
            function requestConversationStarter(slotName) {
              let viafouraAd = document.querySelector('[data-engagement-ad-enable="true"]');
                // Generate next slot name
                var adUnitString = viafouraAd.getAttribute('data-slot-name') || '';

                var sizesAttribute = viafouraAd.getAttribute('data-slot-sizes');
                var sizesArray = JSON.parse(sizesAttribute);

                var sizeMappingString = viafouraAd.getAttribute('data-slot-adsizemap')
                var sizeMappingArray = JSON.parse(sizeMappingString);

                var adTargetingAttribute = viafouraAd.getAttribute('data-targeting')
                var adTargetingArray = JSON.parse(adTargetingAttribute);

                // Define the slot itself, call display() to
                // register the div and refresh() to fetch the ad.
                window.googletag.cmd.push(function() {
                    // Destroy any lingering ad slots if they exist in the widget
                    const slots = window.googletag.pubads().getSlots()
                        .filter((slot) => slot.getSlotElementId() === slotName);
                    window.googletag.destroySlots(slots);
                    // Define ad sizes based on viewport size
                    // NOTE ads are not responsive but new ads will be rendered to the appropriate size
                    // Adapted from https://support.google.com/dfp_premium/answer/4578089?hl=en#responsiveDesign
                    var mapping = googletag.sizeMapping();
                    sizeMappingArray.forEach(function (entry) {
                        var viewportSize = entry[0];
                        var adSizes = entry[1];
                        mapping.addSize(viewportSize, adSizes);
                    });

                    var sizeMapping = mapping.build();

                    // Create a new ad slot, pointing to the slotName given to this function
                    var slot = window.googletag.defineSlot(adUnitString, sizesArray, slotName)
                        .defineSizeMapping(sizeMapping)
                        .addService(window.googletag.pubads());

                    // Add any targeting or any other modifications to the ad here
                    if (adTargetingArray) {
                      for (let key in adTargetingArray) {
                        if (adTargetingArray.hasOwnProperty(key)) {
                          slot.setTargeting(key, adTargetingArray[key])
                        }
                      }
                    }

                    // Display the new ad
                    window.googletag.display(slotName);
                    window.googletag.pubads().refresh([slot]);
                });
            }

            // Subscribe to the viafoura event when the viafoura object is ready
            window.vfQ = window.vfQ || [];

            window.vfQ.push(function() {
                window.vf.$subscribe('vf-ads', 'requestCommentAd', generateCommentAd);
                window.vf.$subscribe('vf-ads', 'requestLiveblogAd', generateLiveblogAd);
                window.vf.$subscribe('vf-ads', 'requestConversationStarter', requestConversationStarter);
            });
        </script>
    


    
        <script type="text/javascript">
    const setupAdmiral = async (publication, environment) => {
        let response = await fetch(`https://ggbepmk06k.execute-api.us-west-2.amazonaws.com/prod?publication=${publication}&environment=${environment}`)
        let rawScript = await response.json()
        let script = document.createElement('script')
        script.innerHTML = rawScript
        document.head.appendChild(script)
    }

    const getPub = () => {
        const domain = window.location.hostname.split('.')
        let name, environment
        if (
            domain.includes('lower') ||
            domain.includes('localhost') ||
            domain.includes('127')
        ) {
            name = 'DEV_TEST'
            environment = 'test'
        } else {
            if (!domain.includes('www')) {
                name = domain[0]
            } else {
                name = domain[1]
                if (name.includes('-')) {
                    name = name.replace('-', '_')
                }
            }
            environment = 'production'
        }
        return { name, environment }
    }

    const pubInfo = getPub()
    setupAdmiral(pubInfo.name, pubInfo.environment)
</script>

    

