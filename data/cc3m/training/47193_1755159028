    if (sectionName == undefined) {
      if (window.location.pathname === '/') {
        sectionName = "Home"
      }
      else {
        let pathname = window.location.pathname.split('/')
        /*
          freeChildPages are free content that falls under a parent section
          e.g. /account/newletters, /podcasts/agweek-podcast, or /obituaries/obits/firstname-lastname-hashkey
        */
        let freeChildPages = ["account", "legal-notices", "obituaries", "obits", "podcasts"]
        sectionName = (freeChildPages.includes(pathname[1]) && typeof(pathname[2]) !== "undefined") ?
            pathname[1] : pathname.slice(-1)[0];
      }
    }

    

    let cannabisCookie = getForumCookie('_fcc_cannabis_session')
    paywall.cannabis = cannabisCookie

    // The content tags
    paywall.tags = js_tags;

    paywall.sectionName = sectionName;
  </script>
