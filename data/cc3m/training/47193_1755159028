<script>
    function addAuth0Info(blueProfile) {
        blueProfile.setValue("auth0_user_id", user.auth0Id);
        blueProfile.setValue("auth0_email", user.email);
        blueConicClient.profile.updateProfile();
    }

    function checkConsentGDPR(blueProfile) {
        const valueCheck = ['iab_purpose_1', 'iab_purpose_3', 'iab_purpose_4', 'iab_purpose_5', 'iab_purpose_6', 'iab_purpose_10', 'usnat_consent_targetedadvertisingoptout', 'usnat_consent_saleoptout', 'usnat_consent_sharingoptout'];
        return valueCheck.some(value => blueProfile.getRefusedObjectives().includes(value));
    }
</script>
    

    
        
