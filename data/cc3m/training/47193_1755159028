      if(evt.data && evt.data === "login") {
        login();
      }
    });

    window.addEventListener("message", function(evt) {
      if(evt.data && evt.data === "register") {
        signup();
      }
    });

    // Created Date
    if("" === "") {
      // The content published date
      paywall.createdDate = "";
    } else {
      // The content published date
      paywall.createdDate = "";
    }

    if (paywall.createdDate) {
      // Article Age (in milliseconds)
      let articleAgeMS = Date.parse(new Date()) - Date.parse(paywall.createdDate);
      // Article Age (in days -- 86400000 ms/day)
      paywall.ageInDays = Math.floor(articleAgeMS/86400000);
    }

    // The content author
    paywall.author = "";

    // The article ID
    paywall.contentId = "0000017d-80c0-dea4-a17d-81fac81e0000";

