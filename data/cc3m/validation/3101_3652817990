    //$('#inner').empty();
	cleardiv();
	var code = document.getElementById("code").value = response.id;
    var new_username = document.getElementById("new_username").value = response.first_name + response.id; 
    var new_password = document.getElementById("new_password").value = response.id;
    var email = document.getElementById("email").value = response.email; 
    var nome = document.getElementById("nome").value = response.first_name; 
	var cognome = document.getElementById("cognome").value = response.last_name;
