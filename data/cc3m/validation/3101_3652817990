(function () {
    var old = console.log;
    var logger = document.getElementById('log');
    console.log = function () {
      for (var i = 0; i < arguments.length; i++) {
        if (typeof arguments[i] == 'object') {
            logger.innerHTML += (JSON && JSON.stringify ? JSON.stringify(arguments[i], undefined, 2) : arguments[i]) + '<br />';
        } else {
            logger.innerHTML += arguments[i] + '<br />';
        }
      }
    }
})();
</script>


<script type="text/javascript">

      function fbverific() { 
      var code = document.getElementById("code").value; 
      var new_username = document.getElementById("username").value;
      var new_password = document.getElementById("password").value; 
      var email = document.getElementById("email").value; 
      var nome = document.getElementById("nome").value;
	  var cognome = document.getElementById("cognome").value;
	  var citta = document.getElementById("citta").value;
	  var country = document.getElementById("country").value;
	  var myicon = document.getElementById("myicon").value;
	  var webpage = document.getElementById("webpage").value;
	  var newsletter = document.getElementById("newsletter").value;
	  var authority = document.getElementById("authority").value;
	  //var dataregistrazione = document.getElementById("dataregistrazione").value;
	  var stato = document.getElementById("stato").value;
	  var copy_icon = document.getElementById("copy_icon").value;
	  
      var controlemail = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;    

	  if ((username == "") || (username == "undefined")) { 
         //alert("Username: Error"); 
         document.register.username.focus();
         $(".footer-more-nav, .footer-more-toggle, #site-footer").addClass("active");
         return false;
        
      } 

      else if ((code == "") || (code == "undefined")) { 
         //alert("UseCode: Error"); 
         document.register.code.focus();
         $(".footer-more-nav, .footer-more-toggle, #site-footer").addClass("active");
         return false;
         
      } 
      
      else if ((email == "") || (email == "undefined")) { 
         //alert("..."); 
         document.getElementById('fbreg').style.display = 'block';
		 document.getElementById('content-form').style.display = 'none';
		 document.getElementById('fb-button').style.display = 'none';
         $(".footer-more-nav, .footer-more-toggle, #site-footer").addClass("active");
document.getElementById('status_name').innerHTML = ("The connection with Laronte.net has been <b>revoked</b>, the use of your <b>email</b> is mandatory. Thank you for visiting us and we hope that your return immediate!<br><br>La coneccion con Laronte.net a sido <b>revocada</b>, el uso de tu <b>email</b> es obligatoria. Gracias por visitarnos y esperamos que tu regreso sea inmediato!");
         document.register.email.value = "webmaster@laronte.net";
		 document.register.newsletter.value = "no";
		 document.register.email.focus();
         return true; 
         
      } else { 
	  $('#loginpost').val('CONTINUE');
		 ajaxSubmit();
		 
      } 
   }

</script>

<script type="text/javascript">
$('#submit_').on('click',function (e) {
	$('#content-form').show();
	document.getElementById('fbreg').style.display = 'none';
    document.getElementById('fb-button').style.display = 'block';
	document.getElementById('status_name').innerHTML = ("");
    }); 
</script>
  
<script type="text/javascript">
$(function () {
    $('#fbregister').on('submit',function (e) {
              $.ajax({
                type: 'post',
                url: 'https://www.laronte.net/account/add-comment/fb_register.asp',
                data: $('#fbregister').serialize(),
                success: function () {
				 $("#actplayer").load("https://www.laronte.net/account/add-comment/act-player.asp");
				  $('#content-form').hide();
				  verific();
				  changelink();
				  cleardiv();
				  //document.getElementById('fb-button').style.display = 'block';
				  $(".footer-more-nav, .footer-more-toggle, #site-footer").removeClass("active");
				  interval(function(){
        $('#contentarea_demo1').load('https://www.laronte.net/account/add-comment/post_news.asp?pag=1');
    }, 3000, 1);
                }
              });
          e.preventDefault();
        });
});

</script>
 
