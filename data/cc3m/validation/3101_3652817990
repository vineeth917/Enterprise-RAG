		document.getElementById('top_avatar_menuB').innerHTML = ('<span id="top_avatar_menuB" class="top_avatar_menu footer-more-toggle openlogin"><a href="#" ><img id="" src=" '+ response.picture.data.url +' " class="img-circle desaturate active_icon_menu" width="35" height="35" ></a></span>');
		document.getElementById('fb-button').style.display = 'none';
		$('#loginpost').val(' CONTINUE ');
		$('#logfb').hide();
