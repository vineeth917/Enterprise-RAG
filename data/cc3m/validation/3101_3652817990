        scrollTop: $("#updatepage").offset().top
    }, 1000);
	
	interval(function(){
        $('#contentarea_demo1').load('https://www.laronte.net/account/add-comment/post_news.asp?pag=1');
    }, 3000, 1);
        return false;
    }); 
	
	//FBconnect();
});

function interval(func, wait, times){
    var interv = function(w, t){
        return function(){
            if(typeof t === "undefined" || t-- > 0){
                setTimeout(interv, w);
                try{
                    func.call(null);
                }
                catch(e){
                    t = 0;
                    throw e.toString();
                }
            }
        };
    }(wait, times);

    setTimeout(interv, wait);
};


 </script>
 

 
 
