<script type="text/javascript">  
$(document).ready(function() {
  $("#loginpost").click(function(){
	  $('html, body').animate(
		{
		scrollTop: $("#updatepage").offset().top
		}, 0);
    var username = $("#username").val();  
    var password = $("#password").val(); 
	
    $.ajax({
      type: "POST",
	  url: "https://www.laronte.net/account/login.asp?step=2",
      data: "username=" + username + "&password=" + password,
      dataType: "html",
	  
      success: function(msg) {
        $("#confermalogin").html(msg);
		$(".footer-more-nav, .footer-more-toggle, #site-footer").removeClass("active");
        verific();
		changelink();
      },
	  
      error: function(){
		  //$("#conferma").html(msg);
      }
    });
  });
});

</script>

<script type="text/javascript">
	function ajaxSubmit() {
    var url = "https://www.laronte.net/account/add-comment/fb_register.asp"; 
    $.ajax({
           type: "POST",
           url: url,
           data: $("#fbregister").serialize(), // serializes the form's elements.
		   success: function(data)
			{
				 $("#actplayer").load("/account/add-comment/act-player.asp");
				 $('#content-form').hide();
				 //$(".footer-more-nav, .footer-more-toggle, #site-footer").removeClass("active");
				  verific();
				  changelink();
				  cleardiv();
				  //interval(function(){
        //$('#contentarea_demo1').load('https://www.laronte.net/account/add-comment/post_news.asp?pag=1');
    //}, 3000, 1);
	
           }
         });
    return false; // avoid to execute the actual submit of the form.
}
</script>

