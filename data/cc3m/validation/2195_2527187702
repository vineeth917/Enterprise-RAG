<script>
	function expertiseHandler(event){
	document.getElementById("sectors-menu").style.display = "block";
	document.getElementById("services-menu").style.display = "block";
	event.target.classList.add("active"); 
	document.getElementById("menu-image-column").classList.add("inactive");
}

jQuery( document ).on( 'elementor/popup/show', ( event, id, instance ) => {
	
	if(id === 490){
		expertiseBtn = document.getElementsByClassName("expertise-link");
		for(let el of expertiseBtn){
			el.addEventListener("click", expertiseHandler);
		}
	}
});
	
	</script>
<script>
const moreDetail = document.getElementById("more-detail");
	
if(moreDetail){
	
const showLess = document.getElementById("show-less");
const productMeta = document.getElementsByClassName("project-meta");

moreDetail.addEventListener("click", ()=>{
    for(let el of productMeta){
        el.style.display = "block";
    }
    moreDetail.style.display = "none";
    showLess.style.display = "block";
    
});


showLess.addEventListener("click", ()=>{

    for(let el of productMeta){
        el.style.display = "none";
    }
    moreDetail.style.display = "block";
    showLess.style.display = "none";
    
});

}

</script>
